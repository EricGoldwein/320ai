[32m+[m[41m    [m
[32m+[m[32m    // Play sound effect if available[m
[32m+[m[32m    // (Sound effects would be added here if we had them)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Add function to spawn collectibles[m
[32m+[m[32mfunction spawnCollectible() {[m
[32m+[m[32m    if (Math.random() < COLLECTIBLE_SPAWN_CHANCE) {[m
[32m+[m[32m        const types = Object.values(COLLECTIBLE_TYPES);[m
[32m+[m[32m        const type = types[Math.floor(Math.random() * types.length)];[m
[32m+[m[41m        [m
[32m+[m[32m        // Randomize height - sometimes higher for jumping challenge[m
[32m+[m[32m        const heightVariation = Math.random() > 0.7 ?[m[41m [m
[32m+[m[32m            Math.random() * 100 : // Higher position (30% chance)[m
[32m+[m[32m            0;                    // Ground level (70% chance)[m
[32m+[m[41m        [m
[32m+[m[32m        const collectible = {[m
[32m+[m[32m            type: type,[m
[32m+[m[32m            x: CANVAS_WIDTH,[m
[32m+[m[32m            y: GROUND_HEIGHT - 30 - heightVariation, // Slightly above ground[m
[32m+[m[32m            width: 30,[m
[32m+[m[32m            height: 30,[m
[32m+[m[32m            rotation: 0,[m
[32m+[m[32m            bobHeight: 0,[m
[32m+[m[32m            bobSpeed: Math.random() * 0.05 + 0.02[m
[32m+[m[32m        };[m
[32m+[m[41m        [m
[32m+[m[32m        collectibles.push(collectible);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Add a win game function[m
[32m+[m[32mfunction winGame() {[m
[32m+[m[32m    gameState.running = false;[m
[32m+[m[41m    [m
[32m+[m[32m    // Create explosion of particles for win effect - more particles[m
[32m+[m[32m    createParticleBurst(daisy.x + daisy.width/2, daisy.y + daisy.height/2, 200, '#FFD700');[m
[32m+[m[41m    [m
[32m+[m[32m    // Update high score if needed[m
[32m+[m[32m    if (gameState.score > gameState.highScore) {[m
[32m+[m[32m        gameState.highScore = gameState.score;[m
[32m+[m[32m        document.getElementById('highscore').textContent = Math.floor(gameState.highScore);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Show win message[m
[32m+[m[32m    showMessage("320 WINGOS! YOU WIN! PRESS ANY KEY", 0);[m
[32m+[m[41m    [m
[32m+[m[32m    // Add more celebratory particles with different colors and patterns[m
[32m+[m[32m    const colors = ['#FF0000', '#00FF00', '#0000FF', '#FFFF00', '#FF00FF', '#00FFFF'];[m
[32m+[m[41m    [m
[32m+[m[32m    // Create firework effect[m
[32m+[m[32m    for (let i = 0; i < 10; i++) {[m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            const x = Math.random() * CANVAS_WIDTH;[m
[32m+[m[32m            const y = Math.random() * (CANVAS_HEIGHT / 2);[m
[32m+[m[32m            const color = colors[Math.floor(Math.random() * colors.length)];[m
[32m+[m[41m            [m
[32m+[m[32m            // Firework explosion[m
[32m+[m[32m            createFirework(x, y, color);[m
[32m+[m[32m        }, i * 300);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Make Daisy do a victory dance[m
[32m+[m[32m    startVictoryDance();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Add a firework effect[m
[32m+[m[32mfunction createFirework(x, y, color) {[m
[32m+[m[32m    // Create a burst with many particles[m
[32m+[m[32m    for (let i = 0; i < 50; i++) {[m
[32m+[m[32m        const angle = Math.random() * Math.PI * 2;[m
[32m+[m[32m        const speed = Math.random() * 5 + 2;[m
[32m+[m[32m        const size = Math.random() * 4 + 2;[m
[32m+[m[41m        [m
[32m+[m[32m        particles.push({[m
[32m+[m[32m            x: x,[m
[32m+[m[32m            y: y,[m
[32m+[m[32m            size: size,[m
[32m+[m[32m            speedX: Math.cos(angle) * speed,[m
[32m+[m[32m            speedY: Math.sin(angle) * speed,[m
[32m+[m[32m            color: color,[m
[32m+[m[32m            life: Math.random() * 50 + 30,[m
[32m+[m[32m            gravity: 0.05[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Play sound effect if available[m
[32m+[m[32m    // (Sound effects would be added here if we had them)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Enhance Daisy's victory dance[m
[32m+[m[32mfunction startVictoryDance() {[m
[32m+[m[32m    // Create a dancing effect for Daisy[m
[32m+[m[32m    const originalY = daisy.y;[m
[32m+[m[32m    let danceStep = 0;[m
[32m+[m[32m    let direction = 1; // For spinning[m
[32m+[m[41m    [m
[32m+[m[32m    const danceInterval = setInterval(() => {[m
[32m+[m[32m        if (gameState.running) {[m
[32m+[m[32m            // Stop dance if game restarted[m
[32m+[m[32m            clearInterval(danceInterval);[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        danceStep++;[m
[32m+[m[41m        [m
[32m+[m[32m        // Make Daisy do a more complex dance[m
[32m+[m[32m        // Jump up and down with varying heights[m
[32m+[m[32m        if (danceStep % 4 < 2) {[m
[32m+[m[32m            // Jump higher every other time[m
[32m+[m[32m            const jumpHeight = (danceStep % 8 < 4) ? 30 : 50;[m
[32m+[m[32m            daisy.y = originalY - jumpHeight;[m
[32m+[m[41m            [m
[32m+[m[32m            // Create particles under Daisy[m
[32m+[m[32m            createParticleBurst([m
[32m+[m[32m                daisy.x + daisy.width/2,[m[41m [m
[32m+[m[32m                daisy.y + daisy.height,[m[41m [m
[32m+[m[32m                8,[m[41m [m
[32m+[m[32m                '#FFD700'[m
[32m+[m[32m            );[m
[32m+[m[32m        } else {[m
[32m+[m[32m            daisy.y = originalY;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Spin around (move left and right)[m
[32m+[m[32m        if (danceStep % 16 === 0) {[m
[32m+[m[32m            direction *= -1; // Change direction[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Move side to side[m
[32m+[m[32m        daisy.x += direction * 5;[m
[32m+[m[41m        [m
[32m+[m[32m        // Create colorful particles around Daisy[m
[32m+[m[32m        if (danceStep % 2 === 0) {[m
[32m+[m[32m            // Create particles in a circle around Daisy[m
[32m+[m[32m            for (let i = 0; i < 3; i++) {[m
[32m+[m[32m                const angle = (danceStep * 0.2) + (i * (Math.PI * 2 / 3));[m
[32m+[m[32m                const distance = 60;[m
[32m+[m[32m                const x = daisy.x + daisy.width/2 + Math.cos(angle) * distance;[m
[32m+[m[32m                const y = daisy.y + daisy.height/2 + Math.sin(angle) * distance;[m
[32m+[m[41m                [m
[32m+[m[32m                createParticleBurst([m
[32m+[m[32m                    x,[m[41m [m
[32m+[m[32m                    y,[m[41m [m
[32m+[m[32m                    5,[m[41m [m
[32m+[m[32m                    `hsl(${(danceStep * 10 + i * 120) % 360}, 100%, 50%)`[m
[32m+[m[32m                );[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Create a trail effect[m
[32m+[m[32m        if (danceStep % 3 === 0) {[m
[32m+[m[32m            createParticleBurst([m
[32m+[m[32m                daisy.x + daisy.width/2,[m[41m [m
[32m+[m[32m                daisy.y + daisy.height - 10,[m[41m [m
[32m+[m[32m                3,[m[41m [m
[32m+[m[32m                `hsl(${danceStep * 5 % 360}, 100%, 70%)`[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Add special effects at certain points[m
[32m+[m[32m        if (danceStep % 10 === 0) {[m
[32m+[m[32m            // Create a burst of particles[m
[32m+[m[32m            createParticleBurst([m
[32m+[m[32m                daisy.x + daisy.width/2,[m
[32m+[m[32m                daisy.y + daisy.height/2,[m
[32m+[m[32m                20,[m
[32m+[m[32m                '#FFFFFF'[m
[32m+[m[32m            );[m
[32m+[m[41m            [m
[32m+[m[32m            // Show celebratory messages[m
[32m+[m[32m            const messages = [[m
[32m+[m[32m                "AWESOME!",[m
[32m+[m[32m                "320 CHAMPION!",[m
[32m+[m[32m                "VICTORY!",[m
[32m+[m[32m                "WINGOS MASTER!",[m
[32m+[m[32m                "LEGENDARY!"[m
[32m+[m[32m            ];[m
[32m+[m[32m            showMessage(messages[Math.floor(Math.random() * messages.length)], 500);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Stop after a longer dance[m
[32m+[m[32m        if (danceStep > 60) {[m
[32m+[m[32m            clearInterval(danceInterval);[m
[32m+[m[32m            daisy.y = originalY;[m
[32m+[m[32m            daisy.x = 50; // Reset to original position[m
[32m+[m[32m        }[m
[32m+[m[32m    }, 150); // Slightly faster for more energetic dance[m
[32m+[m[32m}[m
[32m+[m[32m</script>[m
[32m+[m[32m{% endblock %}[m[41m [m
\ No newline at end of file[m

