[32m+[m[32m// Randomly update metrics[m
[32m+[m[32mfunction updateMetrics() {[m
[32m+[m[32m    const activeNodes = Math.floor(Math.random() * 100) + 280;[m
[32m+[m[32m    const dataPoints = (Math.random() * 0.4 + 3).toFixed(1) + 'M';[m
[32m+[m[32m    const optimizationRate = (Math.random() * 0.9 + 99).toFixed(1) + '%';[m
[32m+[m[41m    [m
[32m+[m[32m    document.getElementById('activeNodes').textContent = activeNodes;[m
[32m+[m[32m    document.getElementById('dataPoints').textContent = dataPoints;[m
[32m+[m[32m    document.getElementById('optimizationRate').textContent = optimizationRate;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Update every 3 seconds[m
[32m+[m[32msetInterval(updateMetrics, 3000);[m
[32m+[m[32m</script>[m
