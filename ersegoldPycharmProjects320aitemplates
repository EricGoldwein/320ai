[32m+[m[32m{% block extra_js %}[m
[32m+[m[32m<script src="https://cdn.jsdelivr.net/npm/phaser@3.55.2/dist/phaser.min.js"></script>[m
[32m+[m[32m<script>[m
[32m+[m[32m// Add error handling for Phaser loading[m
[32m+[m[32mif (typeof Phaser === 'undefined') {[m
[32m+[m[32m    console.error('Phaser is not loaded! Please check your internet connection.');[m
[32m+[m[32m    document.getElementById('game-canvas').innerHTML = '<div style="padding: 20px; color: red;">Error: Game engine failed to load. Please refresh the page.</div>';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Debug loading of assets[m
[32m+[m[32mfunction debugImageLoad(key, success) {[m
[32m+[m[32m    console.log(`Loading ${key}: ${success ? 'Success' : 'Failed'}`);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Game configuration[m
[32m+[m[32mconst config = {[m
[32m+[m[32m    type: Phaser.AUTO,[m
[32m+[m[32m    parent: 'game-canvas',[m
[32m+[m[32m    width: 800,[m
[32m+[m[32m    height: 600,[m
[32m+[m[32m    backgroundColor: '#87CEEB',  // Sky blue background[m
[32m+[m[32m    physics: {[m
[32m+[m[32m        default: 'arcade',[m
[32m+[m[32m        arcade: {[m
[32m+[m[32m            gravity: { y: 0 },  // Remove gravity since this is a side-scroller[m
[32m+[m[32m            debug: true  // Enable debug mode temporarily to see what's happening[m
[32m+[m[32m        }[m
[32m+[m[32m    },[m
[32m+[m[32m    scene: {[m
[32m+[m[32m        preload: preload,[m
[32m+[m[32m        create: create,[m
[32m+[m[32m        update: update[m
[32m+[m[32m    }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Initialize game with error handling[m
[32m+[m[32mtry {[m
[32m+[m[32m    const game = new Phaser.Game(config);[m
[32m+[m[32m} catch (error) {[m
[32m+[m[32m    console.error('Failed to initialize game:', error);[m
[32m+[m[32m    document.getElementById('game-canvas').innerHTML = '<div style="padding: 20px; color: red;">Error: Failed to start game. Please refresh the page.</div>';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Game variables[m
[32m+[m[32mlet player;[m
[32m+[m[32mlet otherRunners = [];[m
[32m+[m[32mlet cursors;[m
[32m+[m[32mlet obstacles;[m
[32m+[m[32mlet wingos = 0;[m
[32m+[m[32mlet distance = 0;[m
[32m+[m[32mlet energy = 100;[m
[32m+[m[32mlet isJumping = false;[m
[32m+[m[32mlet jumpTimer = 0;[m
[32m+[m[32mlet sprintCooldown = false;[m
[32m+[m[32mlet isStretching = false;[m
[32m+[m[32mlet kickCooldown = false;[m
[32m+[m[32mlet currentLane = 1;  // Start in middle lane[m
[32m+[m[32mlet targetY = lanes[currentLane];  // Target Y position for smooth movement[m
[32m+[m[32mlet gameSpeed = 5;[m
[32m+[m[32mlet sprinting = false;[m
[32m+[m[32mlet background;[m
[32m+[m[32mlet lanes = [150, 300, 450];  // Y positions for the three lanes[m
[32m+[m[32mlet jumpHeight = 100;  // Maximum jump height[m
[32m+[m[32mlet isMoving = false;  // Track if player is changing lanes[m
[32m+[m[32mlet gameMessages = [[m
[32m+[m[32m    "No walkers in lane 1!",[m
[32m+[m[32m    "Move to lane 3!",[m
[32m+[m[32m    "Daisy dodges a dog!",[m
[32m+[m[32m    "Daisy stretches mid-run. Elite recovery mode activated!",[m
[32m+[m[32m    "Earn those $Wingos!",[m
[32m+[m[32m    "Watch out for shin splints!",[m
[32m+[m[32m    "Hoof sprain alert!",[m
[32m+[m[32m    "Perfect form, Daisy!"[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32mfunction preload() {[m
[32m+[m[32m    // Add error handlers for asset loading[m
[32m+[m[32m    this.load.on('loaderror', function(file) {[m
[32m+[m[32m        console.error('Error loading asset:', file.src);[m
[32m+[m[32m        debugImageLoad(file.key, false);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.load.on('filecomplete', function(key) {[m
[32m+[m[32m        debugImageLoad(key, true);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Load game assets with error handling[m
[32m+[m[32m    try {[m
[32m+[m[32m        this.load.image('track', "{{ url_for('static', filename='images/game/track.png') }}");[m
[32m+[m[32m        this.load.image('daisy', "{{ url_for('static', filename='images/game/daisy.png') }}");[m
[32m+[m[41m        [m
[32m+[m[32m        // Load obstacle types[m
[32m+[m[32m        this.load.image('dog', "{{ url_for('static', filename='images/game/dog.png') }}");[m
[32m+[m[32m        this.load.image('bike', "{{ url_for('static', filename='images/game/bike.png') }}");[m
[32m+[m[32m        this.load.image('child', "{{ url_for('static', filename='images/game/child.png') }}");[m
[32m+[m[41m        [m
[32m+[m[32m        // Load other runners[m
[32m+[m[32m        for (let i = 1; i <= 3; i++) {[m
[32m+[m[32m            this.load.image(`runner_${i}`, "{{ url_for('static', filename='images/game/') }}" + `runner_${i}.png`);[m
[32m+[m[32m        }[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m        console.error('Error in preload:', error);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction create() {[m
[32m+[m[32m    // Create repeating background[m
[32m+[m[32m    background = this.add.tileSprite(400, 300, 800, 600, 'track');[m
[32m+[m[41m    [m
[32m+[m[32m    // Add Daisy with improved physics[m
[32m+[m[32m    player = this.physics.add.sprite(200, lanes[currentLane], 'daisy');[m
[32m+[m[32m    player.setCollideWorldBounds(true);[m
[32m+[m[32m    player.setSize(32, 32);  // Smaller collision box[m
[32m+[m[32m    player.setOffset(16, 16);  // Center the collision box[m
[32m+[m[32m    player.setBounce(0);  // Remove bounce[m
[32m+[m[32m    player.setScale(0.3);  // Smaller scale to fit lanes better[m
[32m+[m[41m    [m
[32m+[m[32m    // Setup improved controls[m
[32m+[m[32m    cursors = this.input.keyboard.createCursorKeys();[m
[32m+[m[41m    [m
[32m+[m[32m    // Add obstacle group with improved spawning[m
[32m+[m[32m    obstacles = this.physics.add.group();[m
[32m+[m[41m    [m
[32m+[m[32m    // Improved collision detection with grace period[m
[32m+[m[32m    this.physics.add.overlap(player, obstacles, handleCollision, null, this);[m
[32m+[m[41m    [m
[32m+[m[32m    // Start spawning obstacles with longer delay[m
[32m+[m[32m    this.time.addEvent({[m
[32m+[m[32m        delay: 3000,  // Longer delay between obstacles[m
[32m+[m[32m        callback: spawnObstaclePattern,[m
[32m+[m[32m        callbackScope: this,[m
[32m+[m[32m        loop: true[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Energy regeneration[m
[32m+[m[32m    this.time.addEvent({[m
[32m+[m[32m        delay: 100,[m
[32m+[m[32m        callback: regenerateEnergy,[m
[32m+[m[32m        callbackScope: this,[m
[32m+[m[32m        loop: true[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Start random messages[m
[32m+[m[32m    this.time.addEvent({[m
[32m+[m[32m        delay: 5000,[m
[32m+[m[32m        callback: showRandomMessage,[m
[32m+[m[32m        callbackScope: this,[m
[32m+[m[32m        loop: true[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Remove other runners for now to simplify debugging[m
[32m+[m[32m    /*[m
[32m+[m[32m    for (let i = 0; i < 3; i++) {[m
[32m+[m[32m        const runner = this.physics.add.sprite([m
[32m+[m[32m            100 + (i+1) * 150,[m
[32m+[m[32m            lanes[i],[m
[32m+[m[32m            `runner_${i+1}`[m
[32m+[m[32m        );[m
[32m+[m[32m        runner.setCollideWorldBounds(true);[m
[32m+[m[32m        runner.setSize(64, 64);[m
[32m+[m[32m        runner.setOffset(32, 32);[m
[32m+[m[32m        runner.setScale(0.5);[m
[32m+[m[32m        runner.lane = i;[m
[32m+[m[32m        otherRunners.push(runner);[m
[32m+[m[32m    }[m
[32m+[m[32m    */[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction update() {[m
[32m+[m[32m    // Scroll background[m
[32m+[m[32m    background.tilePositionX += gameSpeed * (sprinting ? 1.5 : 1);[m
[32m+[m[41m    [m
[32m+[m[32m    // Update distance[m
[32m+[m[32m    distance += gameSpeed * (sprinting ? 1.5 : 1) / 10;[m
[32m+[m[41m    [m
[32m+[m[32m    // Smooth lane changing[m
[32m+[m[32m    if (!isJumping && !isMoving) {[m
[32m+[m[32m        if (cursors.up.isDown && currentLane > 0) {[m
[32m+[m[32m            isMoving = true;[m
[32m+[m[32m            currentLane--;[m
[32m+[m[32m            this.tweens.add({[m
[32m+[m[32m                targets: player,[m
[32m+[m[32m                y: lanes[currentLane],[m
[32m+[m[32m                angle: -15,  // Tilt up when moving up[m
[32m+[m[32m                duration: 200,[m
[32m+[m[32m                ease: 'Power2',[m
[32m+[m[32m                onComplete: () => {[m
[32m+[m[32m                    this.tweens.add({[m
[32m+[m[32m                        targets: player,[m
[32m+[m[32m                        angle: 0,[m
[32m+[m[32m                        duration: 100[m
[32m+[m[32m                    });[m
[32m+[m[32m                    isMoving = false;[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        } else if (cursors.down.isDown && currentLane < 2) {[m
[32m+[m[32m            isMoving = true;[m
[32m+[m[32m            currentLane++;[m
[32m+[m[32m            this.tweens.add({[m
[32m+[m[32m                targets: player,[m
[32m+[m[32m                y: lanes[currentLane],[m
[32m+[m[32m                angle: 15,  // Tilt down when moving down[m
[32m+[m[32m                duration: 200,[m
[32m+[m[32m                ease: 'Power2',[m
[32m+[m[32m                onComplete: () => {[m
[32m+[m[32m                    this.tweens.add({[m
[32m+[m[32m                        targets: player,[m
[32m+[m[32m                        angle: 0,[m
[32m+[m[32m                        duration: 100[m
[32m+[m[32m                    });[m
[32m+[m[32m                    isMoving = false;[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Handle jumping[m
[32m+[m[32m    if (this.input.keyboard.checkDown(cursors.space, 250) && !isJumping && !kickCooldown) {[m
[32m+[m[32m        isJumping = true;[m
[32m+[m[32m        this.tweens.add({[m
[32m+[m[32m            targets: player,[m
[32m+[m[32m            y: player.y - jumpHeight,[m
[32m+[m[32m            scaleX: 0.6,  // Stretch horizontally[m
[32m+[m[32m            scaleY: 0.4,  // Compress vertically[m
[32m+[m[32m            duration: 300,[m
[32m+[m[32m            ease: 'Power2',[m
[32m+[m[32m            yoyo: true,[m
[32m+[m[32m            onComplete: () => {[m
[32m+[m[32m                isJumping = false;[m
[32m+[m[32m                player.setScale(0.5);  // Reset scale[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Handle sprinting visual effect[m
[32m+[m[32m    if (sprinting) {[m
[32m+[m[32m        player.setAlpha(0.8 + Math.sin(this.time.now / 60) * 0.2);  // Create motion blur effect[m
[32m+[m[32m    } else {[m
[32m+[m[32m        player.setAlpha(1);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Move and rotate obstacles[m
[32m+[m[32m    obstacles.children.iterate(function (obstacle) {[m
[32m+[m[32m        if (obstacle) {[m
[32m+[m[32m            obstacle.x -= gameSpeed * (sprinting ? 1.5 : 1);[m
[32m+[m[32m            obstacle.angle += 2;  // Rotate obstacles[m
[32m+[m[32m            if (obstacle.x < -50) {[m
[32m+[m[32m                obstacle.destroy();[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Update game stats[m
[32m+[m[32m    document.getElementById('distance').textContent = Math.floor(distance);[m
[32m+[m[32m    document.getElementById('wingos').textContent = wingos;[m
[32m+[m[32m    document.getElementById('energy').textContent = Math.floor(energy);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Improved obstacle spawning with variety[m
[32m+[m[32mfunction spawnObstaclePattern() {[m
[32m+[m[32m    const lane = Phaser.Math.Between(0, 2);[m
[32m+[m[32m    const obstacleTypes = ['dog', 'bike', 'child'];[m
[32m+[m[32m    const type = obstacleTypes[Phaser.Math.Between(0, 2)];[m
[32m+[m[41m    [m
[32m+[m[32m    const obstacle = obstacles.create(850, lanes[lane], type);[m
[32m+[m[32m    obstacle.type = type;  // Store the obstacle type[m
[32m+[m[41m    [m
[32m+[m[32m    // Set appropriate scale based on type[m
[32m+[m[32m    let scale = 1.0;[m
[32m+[m[32m    switch(type) {[m
[32m+[m[32m        case 'dog':[m
[32m+[m[32m            scale = Phaser.Math.FloatBetween(0.8, 1.2);[m
[32m+[m[32m            // Add wagging animation[m
[32m+[m[32m            obstacle.scene.tweens.add({[m
[32m+[m[32m                targets: obstacle,[m
[32m+[m[32m                angle: 5,[m
[32m+[m[32m                duration: 200,[m
[32m+[m[32m                yoyo: true,[m
[32m+[m[32m                repeat: -1[m
[32m+[m[32m            });[m
[32m+[m[32m            break;[m
[32m+[m[32m        case 'bike':[m
[32m+[m[32m            scale = Phaser.Math.FloatBetween(1.0, 1.3);[m
[32m+[m[32m            // Add wobble animation[m
[32m+[m[32m            obstacle.scene.tweens.add({[m
[32m+[m[32m                targets: obstacle,[m
[32m+[m[32m                angle: 2,[m
[32m+[m[32m                duration: 100,[m
[32m+[m[32m                yoyo: true,[m
[32m+[m[32m                repeat: -1[m
[32m+[m[32m            });[m
[32m+[m[32m            break;[m
[32m+[m[32m        case 'child':[m
[32m+[m[32m            scale = Phaser.Math.FloatBetween(0.7, 1.0);[m
[32m+[m[32m            // Add bouncing animation[m
[32m+[m[32m            obstacle.scene.tweens.add({[m
[32m+[m[32m                targets: obstacle,[m
[32m+[m[32m                y: obstacle.y - 10,[m
[32m+[m[32m                duration: 500,[m
[32m+[m[32m                yoyo: true,[m
[32m+[m[32m                repeat: -1[m
[32m+[m[32m            });[m
[32m+[m[32m            break;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    obstacle.setScale(scale);[m
[32m+[m[32m    obstacle.setBounce(0);[m
[32m+[m[32m    obstacle.body.allowGravity = false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Improved collision handling with grace period[m
[32m+[m[32mlet lastCollisionTime = 0;[m
[32m+[m[32mfunction handleCollision(player, obstacle) {[m
[32m+[m[32m    const currentTime = Date.now();[m
[32m+[m[32m    if (currentTime - lastCollisionTime < 1000) {[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    let message = "";[m
[32m+[m[32m    let wingoChange = 0;[m
[32m+[m[41m    [m
[32m+[m[32m    if (kickCooldown) {[m
[32m+[m[32m        switch(obstacle.type) {[m
[32m+[m[32m            case 'dog':[m
[32m+[m[32m                message = "Careful with the dog! +5 Wingos";[m
[32m+[m[32m                wingoChange = 5;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'bike':[m
[32m+[m[32m                message = "Bike dodged! +15 Wingos";[m
[32m+[m[32m                wingoChange = 15;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'child':[m
[32m+[m[32m                message = "Child safely avoided! +10 Wingos";[m
[32m+[m[32m                wingoChange = 10;[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m        obstacle.destroy();[m
[32m+[m[32m        wingos += wingoChange;[m
[32m+[m[32m        showMessage(message, '#4CAF50');[m
[32m+[m[32m    } else if (isJumping) {[m
[32m+[m[32m        switch(obstacle.type) {[m
[32m+[m[32m            case 'dog':[m
[32m+[m[32m                message = "Jumped over the dog! +3 Wingos";[m
[32m+[m[32m                wingoChange = 3;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'bike':[m
[32m+[m[32m                message = "Perfect bike jump! +5 Wingos";[m
[32m+[m[32m                wingoChange = 5;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'child':[m
[32m+[m[32m                message = "Safely cleared! +4 Wingos";[m
[32m+[m[32m                wingoChange = 4;[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m        wingos += wingoChange;[m
[32m+[m[32m        showMessage(message, '#2196F3');[m
[32m+[m[32m    } else {[m
[32m+[m[32m        switch(obstacle.type) {[m
[32m+[m[32m            case 'dog':[m
[32m+[m[32m                message = "Watch out for the dog! -5 Wingos";[m
[32m+[m[32m                wingoChange = -5;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'bike':[m
[32m+[m[32m                message = "Bike crash! -15 Wingos";[m
[32m+[m[32m                wingoChange = -15;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'child':[m
[32m+[m[32m                message = "Be careful! -10 Wingos";[m
[32m+[m[32m                wingoChange = -10;[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m        wingos = Math.max(0, wingos + wingoChange);[m
[32m+[m[32m        energy = Math.max(0, energy - 20);[m
[32m+[m[32m        showMessage(message, '#f44336');[m
[32m+[m[32m        lastCollisionTime = currentTime;[m
[32m+[m[41m        [m
[32m+[m[32m        player.setTint(0xff0000);[m
[32m+[m[32m        this.time.delayedCall(100, () => {[m
[32m+[m[32m            player.clearTint();[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    if (!isJumping && !kickCooldown) {[m
[32m+[m[32m        obstacle.destroy();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction regenerateEnergy() {[m
[32m+[m[32m    if (!sprintCooldown && !isStretching) {[m
[32m+[m[32m        energy = Math.min(100, energy + 0.5);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction showMessage(text, color = '#ffffff') {[m
[32m+[m[32m    const messageEl = document.getElementById('game-message');[m
[32m+[m[32m    messageEl.textContent = text;[m
[32m+[m[32m    messageEl.style.color = color;[m
[32m+[m[32m    messageEl.style.display = 'block';[m
[32m+[m[32m    setTimeout(() => {[m
[32m+[m[32m        messageEl.style.display = 'none';[m
[32m+[m[32m    }, 2000);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction showRandomMessage() {[m
[32m+[m[32m    const message = gameMessages[Math.floor(Math.random() * gameMessages.length)];[m
[32m+[m[32m    showMessage(message);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Improved sprinting mechanics[m
[32m+[m[32mdocument.addEventListener('keydown', (event) => {[m
[32m+[m[32m    if (event.key === 'Shift' && !sprintCooldown && energy >= 20) {[m
[32m+[m[32m        sprintCooldown = true;[m
[32m+[m[32m        energy -= 20;[m
[32m+[m[41m        [m
[32m+[m[32m        // Increase track position speed[m
[32m+[m[32m        gameSpeed += 0.02;[m
[32m+[m[41m        [m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            sprintCooldown = false;[m
[32m+[m[32m        }, 3000);[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Improved kicking mechanics with visual feedback[m
[32m+[m[32mdocument.addEventListener('keydown', (event) => {[m
[32m+[m[32m    if (event.code === 'Space' && !kickCooldown && energy >= 10) {[m
[32m+[m[32m        let canKick = false;[m
[32m+[m[32m        obstacles.children.iterate(function (obstacle) {[m
[32m+[m[32m            if (obstacle && Math.abs(obstacle.x - player.x) < 100) {[m
[32m+[m[32m                canKick = true;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        if (canKick) {[m
[32m+[m[32m            kickCooldown = true;[m
[32m+[m[32m            energy -= 10;[m
[32m+[m[41m            [m
[32m+[m[32m            // Add kick animation[m
[32m+[m[32m            player.scene.tweens.add({[m
[32m+[m[32m                targets: player,[m
[32m+[m[32m                angle: 360,  // Full rotation for kick[m
[32m+[m[32m                scaleX: 0.7,[m
[32m+[m[32m                duration: 500,[m
[32m+[m[32m                ease: 'Power2',[m
[32m+[m[32m                onComplete: () => {[m
[32m+[m[32m                    player.setScale(0.5);[m
[32m+[m[32m                    player.angle = 0;[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[41m            [m
[32m+[m[32m            setTimeout(() => {[m
[32m+[m[32m                kickCooldown = false;[m
[32m+[m[32m            }, 1000);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Improved stretching mechanics with visual feedback[m
[32m+[m[32mdocument.addEventListener('keydown', (event) => {[m
[32m+[m[32m    if (event.key.toLowerCase() === 's' && !isStretching) {[m
[32m+[m[32m        isStretching = true;[m
[32m+[m[32m        showMessage("Stretching... Energy boost!", '#FFC107');[m
[32m+[m[41m        [m
[32m+[m[32m        // Add stretch animation[m
[32m+[m[32m        player.scene.tweens.add({[m
[32m+[m[32m            targets: player,[m
[32m+[m[32m            scaleX: 0.7,[m
[32m+[m[32m            scaleY: 0.3,[m
[32m+[m[32m            duration: 500,[m
[32m+[m[32m            yoyo: true,[m
[32m+[m[32m            repeat: 1,[m
[32m+[m[32m            ease: 'Power2',[m
[32m+[m[32m            onComplete: () => {[m
[32m+[m[32m                player.setScale(0.5);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        const recoveryInterval = setInterval(() => {[m
[32m+[m[32m            energy = Math.min(100, energy + 5);[m
[32m+[m[32m        }, 100);[m
[32m+[m[41m        [m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            isStretching = false;[m
[32m+[m[32m            clearInterval(recoveryInterval);[m
[32m+[m[32m        }, 1000);[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
