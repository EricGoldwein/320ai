[32m+[m[32mfrom PIL import Image, ImageDraw[m
[32m+[m[32mimport os[m
[32m+[m
[32m+[m[32mdef create_track_background(width=800, height=600):[m
[32m+[m[32m    """Create a simple track background."""[m
[32m+[m[32m    img = Image.new('RGB', (width, height), '#87CEEB')  # Sky blue[m
[32m+[m[32m    draw = ImageDraw.Draw(img)[m
[32m+[m[41m    [m
[32m+[m[32m    # Green field[m
[32m+[m[32m    draw.rectangle([0, height//2, width, height], fill='#90EE90')[m
[32m+[m[41m    [m
[32m+[m[32m    # Brown track[m
[32m+[m[32m    draw.ellipse([50, 200, width-50, height-50], fill='#8B4513')[m
[32m+[m[32m    draw.ellipse([100, 250, width-100, height-100], fill='#90EE90')[m
[32m+[m[41m    [m
[32m+[m[32m    # White lines[m
[32m+[m[32m    for i in range(6):[m
[32m+[m[32m        draw.ellipse([75+i*10, 225+i*10, width-75-i*10, height-75-i*10], outline='white')[m
[32m+[m[41m    [m
[32m+[m[32m    return img[m
[32m+[m
[32m+[m[32mdef create_horse_sprite(size=128, color='#8B4513'):[m
[32m+[m[32m    """Create a simple horse sprite."""[m
[32m+[m[32m    img = Image.new('RGBA', (size * 8, size), (0, 0, 0, 0))[m
[32m+[m[32m    draw = ImageDraw.Draw(img)[m
[32m+[m[41m    [m
[32m+[m[32m    for frame in range(8):[m
[32m+[m[32m        x_offset = frame * size[m
[32m+[m[41m        [m
[32m+[m[32m        # Body[m
[32m+[m[32m        draw.rectangle([x_offset + 30, 40, x_offset + 90, 80], fill=color)[m
[32m+[m[41m        [m
[32m+[m[32m        # Head[m
[32m+[m[32m        draw.rectangle([x_offset + 10, 30, x_offset + 30, 50], fill=color)[m
[32m+[m[41m        [m
[32m+[m[32m        # Legs (animated)[m
[32m+[m[32m        leg_offset = 10 if frame % 2 == 0 else 0[m
[32m+[m[32m        draw.rectangle([x_offset + 40, 80 + leg_offset, x_offset + 50, 110], fill=color)[m
[32m+[m[32m        draw.rectangle([x_offset + 70, 80 - leg_offset, x_offset + 80, 110], fill=color)[m
[32m+[m[41m        [m
[32m+[m[32m        # Eye[m
[32m+[m[32m        draw.ellipse([x_offset + 15, 35, x_offset + 20, 40], fill='black')[m
[32m+[m[41m    [m
[32m+[m[32m    return img[m
[32m+[m
[32m+[m[32mdef create_obstacle(size=64):[m
[32m+[m[32m    """Create a simple hurdle."""[m
[32m+[m[32m    img = Image.new('RGBA', (size, size), (0, 0, 0, 0))[m
[32m+[m[32m    draw = ImageDraw.Draw(img)[m
[32m+[m[41m    [m
[32m+[m[32m    # Vertical poles[m
[32m+[m[32m    draw.rectangle([10, 10, 20, size-10], fill='brown')[m
[32m+[m[32m    draw.rectangle([size-20, 10, size-10, size-10], fill='brown')[m
[32m+[m[41m    [m
[32m+[m[32m    # Horizontal bar[m
[32m+[m[32m    draw.rectangle([10, 20, size-10, 30], fill='white')[m
[32m+[m[41m    [m
[32m+[m[32m    return img[m
[32m+[m
[32m+[m[32mdef main():[m
[32m+[m[32m    # Create directory[m
[32m+[m[32m    os.makedirs('static/images/game', exist_ok=True)[m
[32m+[m[41m    [m
[32m+[m[32m    # Create and save track[m
[32m+[m[32m    track = create_track_background()[m
[32m+[m[32m    track.save('static/images/game/track.png')[m
[32m+[m[41m    [m
[32m+[m[32m    # Create and save Daisy[m
[32m+[m[32m    daisy = create_horse_sprite(color='#8B4513')  # Brown[m
[32m+[m[32m    daisy.save('static/images/game/daisy.png')[m
[32m+[m[41m    [m
[32m+[m[32m    # Create and save other runners[m
[32m+[m[32m    colors = ['#8B0000', '#006400', '#4B0082']  # Dark red, dark green, indigo[m
[32m+[m[32m    for i, color in enumerate(colors):[m
[32m+[m[32m        runner = create_horse_sprite(color=color)[m
[32m+[m[32m        runner.save(f'static/images/game/runner_{i+1}.png')[m
[32m+[m[41m    [m
[32m+[m[32m    # Create and save obstacle[m
[32m+[m[32m    obstacle = create_obstacle()[m
[32m+[m[32m    obstacle.save('static/images/game/obstacle.png')[m
[32m+[m[41m    [m
[32m+[m[32m    print("Asset creation complete!")[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    main()[m[41m [m
