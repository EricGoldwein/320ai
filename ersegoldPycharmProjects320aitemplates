[32m+[m[32m{% block extra_js %}[m
[32m+[m[32m<script>[m
[32m+[m[32mlet currentQuestion = 1;[m
[32m+[m[32mconst totalQuestions = 1; // Only requiring name field[m
[32m+[m
[32m+[m[32mfunction validateInput() {[m
[32m+[m[32m    const userName = document.getElementById('userName').value;[m
[32m+[m[32m    return userName.trim() !== '';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction nextQuestion() {[m
[32m+[m[32m    if (currentQuestion === 1) {[m
[32m+[m[32m        if (!validateInput()) {[m
[32m+[m[32m            alert('Please enter your name to continue.');[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    document.querySelector(`.question${currentQuestion}`).classList.remove('active');[m
[32m+[m[32m    currentQuestion++;[m
[32m+[m[41m    [m
[32m+[m[32m    if (currentQuestion <= totalQuestions) {[m
[32m+[m[32m        document.querySelector(`.question${currentQuestion}`).classList.add('active');[m
[32m+[m[32m    } else {[m
[32m+[m[32m        generateWorkout();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction prevQuestion() {[m
[32m+[m[32m    if (currentQuestion > 1) {[m
[32m+[m[32m        document.querySelector(`.question${currentQuestion}`).classList.remove('active');[m
[32m+[m[32m        currentQuestion--;[m
[32m+[m[32m        document.querySelector(`.question${currentQuestion}`).classList.add('active');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32masync function generateWorkout() {[m
[32m+[m[32m    // Hide questionnaire and show loading[m
[32m+[m[32m    document.getElementById('questionnaireForm').style.display = 'none';[m
[32m+[m[32m    document.getElementById('loadingSection').style.display = 'block';[m
[32m+[m[32m    document.getElementById('resultSection').style.display = 'none';[m
[32m+[m
[32m+[m[32m    // Update progress bar and status messages[m
[32m+[m[32m    const progressBar = document.getElementById('progressBar');[m
[32m+[m[32m    const statusText = document.getElementById('statusText');[m
[32m+[m[32m    const quantumMessages = [[m
[32m+[m[32m        "Quantum-calibrating your stride length...",[m
[32m+[m[32m        "Neural networks analyzing your running aura...",[m
[32m+[m[32m        "Calculating optimal track position using string theory...",[m
[32m+[m[32m        "Engaging 320-dimensional hyperspace analysis...",[m
[32m+[m[32m        "Converting caffeine levels to quantum states..."[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    // Simulate loading with quantum messages[m
[32m+[m[32m    for (let i = 0; i < quantumMessages.length; i++) {[m
[32m+[m[32m        await new Promise(resolve => setTimeout(resolve, 800));[m
[32m+[m[32m        const progress = ((i + 1) / quantumMessages.length) * 100;[m
[32m+[m[32m        progressBar.style.width = `${progress}%`;[m
[32m+[m[32m        statusText.textContent = quantumMessages[i];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Prepare form data[m
[32m+[m[32m    const formData = {[m
[32m+[m[32m        name: document.getElementById('userName').value,[m
[32m+[m[32m        miles_past_320_days: "320",[m
[32m+[m[32m        kilometers_2025: "3200",[m
[32m+[m[32m        avg_hr_walking: "72",[m
[32m+[m[32m        recent_haircut: "32",[m
[32m+[m[32m        iron_levels: "32",[m
[32m+[m[32m        last_race_date: "2024-03-20",[m
[32m+[m[32m        last_date_race: "2024-03-20",[m
[32m+[m[32m        ssn_digit_sum: "16"[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m        // Make API call[m
[32m+[m[32m        const response = await fetch('/generate_workout', {[m
[32m+[m[32m            method: 'POST',[m
[32m+[m[32m            headers: {[m
[32m+[m[32m                'Content-Type': 'application/json',[m
[32m+[m[32m            },[m
[32m+[m[32m            body: JSON.stringify(formData)[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        if (!response.ok) {[m
[32m+[m[32m            throw new Error('Network response was not ok');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        const data = await response.json();[m
[32m+[m
[32m+[m[32m        // Hide loading and show results[m
[32m+[m[32m        document.getElementById('loadingSection').style.display = 'none';[m
[32m+[m[32m        document.getElementById('resultSection').style.display = 'block';[m
[32m+[m
[32m+[m[32m        // Display workout details[m
[32m+[m[32m        document.getElementById('workoutIntro').textContent = data.workout_intro;[m
[32m+[m[32m        document.getElementById('workoutDetails').innerHTML = `[m
[32m+[m[32m            <div class="workout-card">[m
[32m+[m[32m                <h3>${data.workout_details.intensity}</h3>[m
[32m+[m[32m                <div class="intensity-badge">${data.workout_details.intensity}</div>[m
[32m+[m[32m                <pre>${data.workout_details.workout}</pre>[m
[32m+[m[32m                <div class="science">${data.workout_details.science}</div>[m
[32m+[m[32m                <div class="meta">[m
[32m+[m[32m                    <span><i class="fas fa-brain"></i>Models: ${data.meta.models_consulted}</span>[m
[32m+[m[32m                    <span><i class="fas fa-clock"></i>Processing: ${data.meta.processing_time_ms}ms</span>[m
[32m+[m[32m                    <span><i class="fas fa-atom"></i>Quantum States: ${data.meta.quantum_states_analyzed}</span>[m
[32m+[m[32m                </div>[m
[32m+[m[32m            </div>[m
[32m+[m[32m        `;[m
[32m+[m[32m        document.getElementById('aiInsight').textContent = data.ai_insight;[m
[32m+[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m        console.error('Error:', error);[m
[32m+[m[32m        alert('Failed to generate workout. Please try again.');[m
[32m+[m[32m        document.getElementById('loadingSection').style.display = 'none';[m
[32m+[m[32m        document.getElementById('questionnaireForm').style.display = 'block';[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction startOver() {[m
[32m+[m[32m    currentQuestion = 1;[m
[32m+[m[32m    document.getElementById('userName').value = '';[m
[32m+[m[32m    document.getElementById('resultSection').style.display = 'none';[m
[32m+[m[32m    document.getElementById('questionnaireForm').style.display = 'block';[m
[32m+[m[32m    document.querySelectorAll('.question-section').forEach(section => section.classList.remove('active'));[m
[32m+[m[32m    document.querySelector('.question1').classList.add('active');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Initialize first question[m
[32m+[m[32mdocument.addEventListener('DOMContentLoaded', () => {[m
[32m+[m[32m    document.querySelector('.question1').classList.add('active');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Email signup form handler[m
[32m+[m[32mdocument.getElementById('emailSignupForm').addEventListener('submit', function(e) {[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m[32m    const emailInput = document.querySelector('input[type="email"]');[m
[32m+[m[32m    const submitButton = this.querySelector('button[type="submit"]');[m
[32m+[m[32m    const messageDiv = document.getElementById('signupMessage');[m
[32m+[m[32m    const email = emailInput.value;[m
[32m+[m
[32m+[m[32m    // Disable form while submitting[m
[32m+[m[32m    emailInput.disabled = true;[m
[32m+[m[32m    submitButton.disabled = true;[m
[32m+[m[32m    submitButton.innerHTML = 'Processing...';[m
[32m+[m
[32m+[m[32m    // Send to backend[m
[32m+[m[32m    fetch('/subscribe', {[m
[32m+[m[32m        method: 'POST',[m
[32m+[m[32m        headers: {[m
[32m+[m[32m            'Content-Type': 'application/json',[m
[32m+[m[32m        },[m
[32m+[m[32m        body: JSON.stringify({ email: email })[m
[32m+[m[32m    })[m
[32m+[m[32m    .then(response => response.json())[m
[32m+[m[32m    .then(data => {[m
[32m+[m[32m        if (data.success) {[m
[32m+[m[32m            showMessage('success', 'You have been added to the waiting list! We will notify you when a spot becomes available.');[m
[32m+[m[32m            this.reset();[m
[32m+[m[32m        } else {[m
[32m+[m[32m            if (data.message.includes('already subscribed')) {[m
[32m+[m[32m                showMessage('info', 'You are already on the waiting list! We will notify you when a spot becomes available.');[m
[32m+[m[32m            } else {[m
[32m+[m[32m                showMessage('error', data.message || 'Something went wrong. Please try again.');[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    })[m
[32m+[m[32m    .catch(error => {[m
[32m+[m[32m        showMessage('success', 'You have been added to the waiting list! We will notify you when a spot becomes available.');[m
[32m+[m[32m        console.error('Error:', error);[m
[32m+[m[32m    })[m
[32m+[m[32m    .finally(() => {[m
[32m+[m[32m        // Re-enable form[m
[32m+[m[32m        emailInput.disabled = false;[m
[32m+[m[32m        submitButton.disabled = false;[m
[32m+[m[32m        submitButton.innerHTML = 'Subscribe';[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mfunction showMessage(type, message) {[m
[32m+[m[32m    const messageDiv = document.getElementById('signupMessage');[m
[32m+[m[32m    messageDiv.style.display = 'block';[m
[32m+[m[32m    messageDiv.className = `alert alert-${type === 'error' ? 'danger' : type === 'info' ? 'info' : 'success'} mt-3`;[m
[32m+[m[32m    messageDiv.innerHTML = message;[m
[32m+[m[32m}[m
[32m+[m[32m</script>[m
[32m+[m[32m{% endblock %}[m[41m [m
