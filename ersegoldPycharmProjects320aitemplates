[32m+[m[32mfrom flask import Flask, request, jsonify, render_template, url_for, send_from_directory[m
[32m+[m[32mimport random[m
[32m+[m[32mfrom datetime import datetime[m
[32m+[m[32mimport sqlite3[m
[32m+[m[32mimport os[m
[32m+[m
[32m+[m[32mapp = Flask(__name__, static_folder='static', static_url_path='/static')[m
[32m+[m
[32m+[m[32m# Add a route to serve static files directly[m
[32m+[m[32m@app.route('/static/<path:filename>')[m
[32m+[m[32mdef serve_static(filename):[m
[32m+[m[32m    return send_from_directory(app.static_folder, filename)[m
[32m+[m
[32m+[m[32m# Database setup[m
[32m+[m[32mdef init_db():[m
[32m+[m[32m    db_path = os.path.join(app.root_path, 'subscribers.db')[m
[32m+[m[32m    conn = sqlite3.connect(db_path)[m
[32m+[m[32m    c = conn.cursor()[m
[32m+[m[32m    c.execute('''[m
[32m+[m[32m        CREATE TABLE IF NOT EXISTS subscribers[m
[32m+[m[32m        (email TEXT PRIMARY KEY, signup_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP)[m
[32m+[m[32m    ''')[m
[32m+[m[32m    conn.commit()[m
[32m+[m[32m    conn.close()[m
[32m+[m
[32m+[m[32m# Initialize database on startup[m
[32m+[m[32mwith app.app_context():[m
[32m+[m[32m    init_db()[m
[32m+[m
