[1mdiff --git a/static/images/game/daisy.svg b/static/images/game/daisy.svg[m
[1mnew file mode 100644[m
[1mindex 0000000..5fa0203[m
[1m--- /dev/null[m
[1m+++ b/static/images/game/daisy.svg[m
[36m@@ -0,0 +1,50 @@[m
[32m+[m[32m<?xml version="1.0" encoding="UTF-8"?>[m
[32m+[m[32m<svg width="1024" height="128" xmlns="http://www.w3.org/2000/svg">[m
[32m+[m[32m    <!-- Frame 1: Standing -->[m
[32m+[m[32m    <g transform="translate(0,0)">[m
[32m+[m[32m        <rect x="32" y="16" width="64" height="96" fill="#8e44ad"/>[m
[32m+[m[32m        <circle cx="64" cy="16" r="16" fill="#9b59b6"/>[m
[32m+[m[32m    </g>[m
[32m+[m[41m    [m
