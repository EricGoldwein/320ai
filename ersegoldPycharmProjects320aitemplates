[32m+[m[32m/* Animations */[m
[32m+[m[32m@keyframes streamData {[m
[32m+[m[32m    0% { transform: translateX(0); opacity: 1; }[m
[32m+[m[32m    100% { transform: translateX(400%); opacity: 0; }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@keyframes fillProgress {[m
[32m+[m[32m    from { stroke-dashoffset: 100; }[m
[32m+[m[32m    to { stroke-dashoffset: 25; }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@keyframes waveMotion {[m
[32m+[m[32m    0% { transform: translateX(-50%) rotate(0deg); }[m
[32m+[m[32m    100% { transform: translateX(-50%) rotate(360deg); }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@keyframes pulseRing {[m
[32m+[m[32m    0% { transform: translate(-50%, -50%) scale(0.8); opacity: 0.8; }[m
[32m+[m[32m    50% { transform: translate(-50%, -50%) scale(1.1); opacity: 0.4; }[m
