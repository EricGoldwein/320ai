[1m--- /dev/null[m
[1m+++ b/create_track.py[m
[36m@@ -0,0 +1,33 @@[m
[32m+[m[32mfrom PIL import Image, ImageDraw[m
[32m+[m
[32m+[m[32mdef create_track():[m
[32m+[m[32m    # Create a large image for seamless tiling[m
[32m+[m[32m    width = 800[m
[32m+[m[32m    height = 600[m
[32m+[m[41m    [m
[32m+[m[32m    img = Image.new('RGBA', (width, height), (135, 206, 235))  # Sky blue background[m
[32m+[m[32m    draw = ImageDraw.Draw(img)[m
[32m+[m[41m    [m
[32m+[m[32m    # Draw grass[m
[32m+[m[32m    draw.rectangle([0, height//2, width, height], fill=(34, 139, 34))  # Forest green[m
[32m+[m[41m    [m
[32m+[m[32m    # Draw track lanes[m
[32m+[m[32m    lane_colors = [(210, 180, 140), (200, 170, 130), (190, 160, 120)]  # Different shades of tan[m
[32m+[m[32m    lane_heights = [150, 300, 450][m
[32m+[m[32m    lane_width = 100[m
[32m+[m[41m    [m
[32m+[m[32m    for i, y in enumerate(lane_heights):[m
[32m+[m[32m        draw.rectangle([0, y - lane_width//2, width, y + lane_width//2],[m[41m [m
[32m+[m[32m                      fill=lane_colors[i])[m
[32m+[m[41m        [m
[32m+[m[32m        # Add lane markers[m
[32m+[m[32m        marker_width = 20[m
[32m+[m[32m        for x in range(0, width, 100):[m
[32m+[m[32m            draw.rectangle([x, y-2, x+marker_width, y+2],[m[41m [m
[32m+[m[32m                         fill=(255, 255, 255))[m
[32m+[m[41m    [m
[32m+[m[32m    # Save the image[m
[32m+[m[32m    img.save('static/images/game/track.png')[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    create_track()[m[41m [m
\ No newline at end of file[m
[1mdiff --git a/download_assets.py b/download_assets.py[m
[1mnew file mode 100644[m
[1mindex 0000000..789d196[m
[1m--- /dev/null[m
[1m+++ b/download_assets.py[m
[36m@@ -0,0 +1,36 @@[m
[32m+[m[32mimport os[m
[32m+[m[32mimport requests[m
[32m+[m[32mfrom pathlib import Path[m
[32m+[m
[32m+[m[32mdef download_file(url, filename):[m
[32m+[m[32m    response = requests.get(url)[m
[32m+[m[32m    if response.status_code == 200:[m
[32m+[m[32m        with open(filename, 'wb') as f:[m
[32m+[m[32m            f.write(response.content)[m
[32m+[m[32m        print(f"Downloaded {filename}")[m
[32m+[m[32m    else:[m
[32m+[m[32m        print(f"Failed to download {filename}")[m
[32m+[m
[32m+[m[32mdef main():[m
[32m+[m[32m    # Create game2 directory if it doesn't exist[m
[32m+[m[32m    game2_dir = Path('static/images/game2')[m
[32m+[m[32m    game2_dir.mkdir(parents=True, exist_ok=True)[m
[32m+[m
[32m+[m[32m    # Asset URLs (these are placeholder URLs - I'll replace them with actual asset URLs)[m
[32m+[m[32m    assets = {[m
[32m+[m[32m        'daisy.png': 'https://raw.githubusercontent.com/your-repo/pixel-horse/main/horse.png',[m
[32m+[m[32m        'porcupine.png': 'https://raw.githubusercontent.com/your-repo/pixel-porcupine/main/porcupine.png',[m
[32m+[m[32m        'snake.png': 'https://raw.githubusercontent.com/your-repo/pixel-snake/main/snake.png',[m
[32m+[m[32m        'horse.png': 'https://raw.githubusercontent.com/your-repo/pixel-horse-2/main/horse.png',[m
[32m+[m[32m        'yellowstone.png': 'https://raw.githubusercontent.com/your-repo/pixel-mountains/main/mountains.png',[m
[32m+[m[32m        'river.png': 'https://raw.githubusercontent.com/your-repo/pixel-river/main/river.png',[m
[32m+[m[32m        'grass.png': 'https://raw.githubusercontent.com/your-repo/pixel-grass/main/grass.png'[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    # Download each asset[m
[32m+[m[32m    for filename, url in assets.items():[m
[32m+[m[32m        filepath = game2_dir / filename[m
[32m+[m[32m        download_file(url, filepath)[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    main()[m[41m [m
