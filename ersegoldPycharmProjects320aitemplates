[32m+[m[32m{% block extra_js %}[m
[32m+[m[32m<script>[m
[32m+[m[32m// Timer functionality[m
[32m+[m[32mlet timerInterval;[m
[32m+[m[32mlet startTime;[m
[32m+[m[32mlet isRunning = false;[m
[32m+[m
[32m+[m[32mfunction formatTime(ms) {[m
[32m+[m[32m    const seconds = Math.floor(ms / 1000);[m
[32m+[m[32m    const minutes = Math.floor(seconds / 60);[m
[32m+[m[32m    const remainingSeconds = seconds % 60;[m
[32m+[m[32m    return `${minutes.toString().padStart(2, '0')}:${remainingSeconds.toString().padStart(2, '0')}`;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction startTimer() {[m
[32m+[m[32m    const startSound = document.getElementById('startSound');[m
[32m+[m[32m    startSound.play();[m
[32m+[m[41m    [m
[32m+[m[32m    startTime = Date.now();[m
[32m+[m[32m    isRunning = true;[m
[32m+[m[32m    document.querySelector('.start-timer').style.display = 'none';[m
[32m+[m[32m    document.querySelector('.stop-timer').style.display = 'inline-block';[m
[32m+[m[41m    [m
[32m+[m[32m    timerInterval = setInterval(() => {[m
[32m+[m[32m        const elapsed = Date.now() - startTime;[m
[32m+[m[32m        document.querySelector('.timer-display').textContent = formatTime(elapsed);[m
[32m+[m[41m        [m
[32m+[m[32m        // Update progress ring[m
[32m+[m[32m        const progress = (elapsed / 320000) * 100;[m
[32m+[m[32m        const circle = document.querySelector('.progress-ring__circle');[m
[32m+[m[32m        const radius = circle.r.baseVal.value;[m
[32m+[m[32m        const circumference = radius * 2 * Math.PI;[m
[32m+[m[32m        const offset = circumference - (progress / 100) * circumference;[m
[32m+[m[32m        circle.style.strokeDasharray = `${circumference} ${circumference}`;[m
[32m+[m[32m        circle.style.strokeDashoffset = offset;[m
[32m+[m[32m    }, 10);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction stopTimer() {[m
[32m+[m[32m    const stopSound = document.getElementById('stopSound');[m
[32m+[m[32m    stopSound.play();[m
[32m+[m[41m    [m
[32m+[m[32m    clearInterval(timerInterval);[m
[32m+[m[32m    isRunning = false;[m
[32m+[m[32m    const elapsed = Date.now() - startTime;[m
[32m+[m[32m    const elapsedSeconds = Math.floor(elapsed / 1000);[m
[32m+[m[32m    const difference = Math.abs(320 - elapsedSeconds);[m
[32m+[m[41m    [m
[32m+[m[32m    const resultMessage = document.querySelector('.result-message');[m
[32m+[m[32m    resultMessage.style.display = 'block';[m
[32m+[m[41m    [m
[32m+[m[32m    if (difference <= 7) {[m
[32m+[m[32m        const successSound = document.getElementById('successSound');[m
[32m+[m[32m        successSound.play();[m
[32m+[m[32m        resultMessage.innerHTML = `[m
[32m+[m[32m            <div class="alert alert-success">[m
[32m+[m[32m                <i class="fas fa-check-circle"></i>[m
[32m+[m[32m                Great job! You were off by only ${difference} seconds.[m
[32m+[m[32m                Your time: ${formatTime(elapsed)}[m
[32m+[m[32m            </div>[m
[32m+[m[32m        `;[m
[32m+[m[32m    } else {[m
[32m+[m[32m        const failSound = document.getElementById('failSound');[m
[32m+[m[32m        failSound.play();[m
[32m+[m[32m        resultMessage.innerHTML = `[m
[32m+[m[32m            <div class="alert alert-danger">[m
[32m+[m[32m                <i class="fas fa-times-circle"></i>[m
[32m+[m[32m                You were off by ${difference} seconds. Try again![m
[32m+[m[32m                Your time: ${formatTime(elapsed)}[m
[32m+[m[32m            </div>[m
[32m+[m[32m        `;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    document.querySelector('.start-timer').style.display = 'inline-block';[m
[32m+[m[32m    document.querySelector('.stop-timer').style.display = 'none';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mdocument.querySelector('.start-timer').addEventListener('click', startTimer);[m
[32m+[m[32mdocument.querySelector('.stop-timer').addEventListener('click', stopTimer);[m
[32m+[m
[32m+[m[32m// Runner Animation[m
[32m+[m[32mconst runnerSprite = document.querySelector('.runner-sprite');[m
[32m+[m[32mlet runnerPosition = 0;[m
[32m+[m[32mconst spriteWidth = 50;[m
[32m+[m[32mconst spriteCount = 8;[m
[32m+[m
[32m+[m[32mfunction animateRunner() {[m
[32m+[m[32m    runnerPosition = (runnerPosition + 1) % spriteCount;[m
[32m+[m[32m    runnerSprite.style.backgroundPosition = `-${runnerPosition * spriteWidth}px 0`;[m
[32m+[m[32m    requestAnimationFrame(animateRunner);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32manimateRunner();[m
[32m+[m
[32m+[m[32m// Backwards Runner Animation[m
[32m+[m[32mconst backwardsSprite = document.querySelector('.backwards-sprite');[m
[32m+[m[32mlet backwardsPosition = 0;[m
[32m+[m
[32m+[m[32mfunction animateBackwardsRunner() {[m
[32m+[m[32m    backwardsPosition = (backwardsPosition + 1) % spriteCount;[m
[32m+[m[32m    backwardsSprite.style.backgroundPosition = `-${backwardsPosition * spriteWidth}px 0`;[m
[32m+[m[32m    requestAnimationFrame(animateBackwardsRunner);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32manimateBackwardsRunner();[m
[32m+[m
[32m+[m[32m// Workout interaction functions[m
[32m+[m[32mfunction toggleWorkoutDetails(workout) {[m
[32m+[m[32m    const details = document.getElementById(`${workout}-details`);[m
[32m+[m[32m    details.classList.toggle('active');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Plank Timer[m
[32m+[m[32mlet plankInterval;[m
[32m+[m[32mlet plankStartTime;[m
[32m+[m
[32m+[m[32mfunction startPlank() {[m
[32m+[m[32m    const startBtn = document.querySelector('.start-plank');[m
[32m+[m[32m    const stopBtn = document.querySelector('.stop-plank');[m
[32m+[m[32m    const progress = document.querySelector('.plank-progress');[m
[32m+[m[41m    [m
[32m+[m[32m    startBtn.style.display = 'none';[m
[32m+[m[32m    stopBtn.style.display = 'inline-block';[m
[32m+[m[32m    progress.style.display = 'block';[m
[32m+[m[41m    [m
[32m+[m[32m    plankStartTime = Date.now();[m
[32m+[m[41m    [m
[32m+[m[32m    plankInterval = setInterval(() => {[m
[32m+[m[32m        const elapsed = Date.now() - plankStartTime;[m
[32m+[m[32m        const remaining = 320000 - elapsed;[m
[32m+[m[41m        [m
[32m+[m[32m        if (remaining <= 0) {[m
[32m+[m[32m            clearInterval(plankInterval);[m
[32m+[m[32m            document.querySelector('.plank-display').textContent = '00:00';[m
[32m+[m[32m            alert('Congratulations! You completed the 320-second plank!');[m
[32m+[m[32m            startBtn.style.display = 'inline-block';[m
[32m+[m[32m            stopBtn.style.display = 'none';[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        const progress = (elapsed / 320000) * 100;[m
[32m+[m[32m        document.querySelector('.progress-bar').style.width = `${progress}%`;[m
[32m+[m[41m        [m
[32m+[m[32m        const seconds = Math.ceil(remaining / 1000);[m
[32m+[m[32m        const minutes = Math.floor(seconds / 60);[m
[32m+[m[32m        const remainingSeconds = seconds % 60;[m
[32m+[m[32m        document.querySelector('.plank-display').textContent =[m[41m [m
[32m+[m[32m            `${minutes.toString().padStart(2, '0')}:${remainingSeconds.toString().padStart(2, '0')}`;[m
[32m+[m[32m    }, 100);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction stopPlank() {[m
[32m+[m[32m    clearInterval(plankInterval);[m
[32m+[m[32m    document.querySelector('.start-plank').style.display = 'inline-block';[m
[32m+[m[32m    document.querySelector('.stop-plank').style.display = 'none';[m
[32m+[m[32m    document.querySelector('.plank-display').textContent = '05:20';[m
[32m+[m[32m    document.querySelector('.progress-bar').style.width = '0%';[m
[32m+[m[32m    document.querySelector('.plank-progress').style.display = 'none';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mdocument.querySelector('.start-plank')?.addEventListener('click', startPlank);[m
[32m+[m[32mdocument.querySelector('.stop-plank')?.addEventListener('click', stopPlank);[m
[32m+[m
[32m+[m[32m// ERock's 160 Workout Progress[m
[32m+[m[32mfunction startErockWorkout() {[m
[32m+[m[32m    const steps = document.querySelectorAll('.progress-step');[m
[32m+[m[32m    let currentStep = 0;[m
[32m+[m[41m    [m
[32m+[m[32m    function updateProgress() {[m
[32m+[m[32m        if (currentStep >= steps.length) {[m
[32m+[m[32m            alert('Congratulations! You completed ERock\'s 160!');[m
[32m+[m[32m            steps.forEach(step => step.classList.remove('completed'));[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        steps[currentStep].classList.add('completed');[m
[32m+[m[32m        currentStep++;[m
[32m+[m[41m        [m
[32m+[m[32m        if (currentStep < steps.length) {[m
[32m+[m[32m            setTimeout(() => {[m
[32m+[m[32m                if (confirm('Ready for the next round?')) {[m
[32m+[m[32m                    updateProgress();[m
[32m+[m[32m                }[m
[32m+[m[32m            }, 1000);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            updateProgress();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    if (confirm('Ready to start ERock\'s 160?')) {[m
[32m+[m[32m        updateProgress();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Song Roulette[m
[32m+[m[32mconst songs = [[m
[32m+[m[32m    { name: "Fighter - Christina Aguilera", bpm: 170 },[m
[32m+[m[32m    { name: "Genie in a Bottle - Christina Aguilera", bpm: 120 },[m
[32m+[m[32m    { name: "Beautiful - Christina Aguilera", bpm: 98 },[m
[32m+[m[32m    { name: "Dirrty - Christina Aguilera", bpm: 100 },[m
[32m+[m[32m    { name: "Lady Marmalade - Christina Aguilera", bpm: 125 },[m
[32m+[m[32m    { name: "Ain't No Other Man - Christina Aguilera", bpm: 132 }[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32mfunction spinRoulette() {[m
[32m+[m[32m    const song = songs[Math.floor(Math.random() * songs.length)];[m
[32m+[m[32m    const songDisplay = document.querySelector('.current-song');[m
[32m+[m[32m    songDisplay.innerHTML = `[m
[32m+[m[32m        <strong>${song.name}</strong><br>[m
[32m+[m[32m        BPM: ${song.bpm}<br>[m
[32m+[m[32m        <small>Adjust your pace to match the beat!</small>[m
[32m+[m[32m    `;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction processQuestion1() {[m
[32m+[m[32m    const userName = document.getElementById('userName').value;[m
[32m+[m[32m    if (userName.trim() === '') {[m
[32m+[m[32m        alert('Please enter your designation');[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Update verification status[m
[32m+[m[32m    const statusValues = document.querySelectorAll('.status-value');[m
[32m+[m[32m    statusValues[0].textContent = 'VERIFIED';[m
[32m+[m[32m    statusValues[0].style.color = '#00ff00';[m
[32m+[m[32m    setTimeout(() => {[m
[32m+[m[32m        statusValues[1].textContent = 'CONNECTED';[m
[32m+[m[32m        statusValues[1].style.color = '#00ff00';[m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            statusValues[2].textContent = 'COMPLETE';[m
[32m+[m[32m            statusValues[2].style.color = '#00ff00';[m
[32m+[m[32m            setTimeout(() => {[m
[32m+[m[32m                nextQuestion(1);[m
[32m+[m[32m            }, 500);[m
[32m+[m[32m        }, 500);[m
[32m+[m[32m    }, 500);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction nextQuestion(current) {[m
[32m+[m[32m    document.getElementById(`q${current}`).classList.remove('active');[m
[32m+[m[32m    document.getElementById(`q${current + 1}`).classList.add('active');[m
[32m+[m[32m    document.getElementById(`q${current + 1}`).scrollIntoView({ behavior: 'smooth' });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction prevQuestion(current) {[m
[32m+[m[32m    document.getElementById(`q${current}`).classList.remove('active');[m
[32m+[m[32m    document.getElementById(`q${current - 1}`).classList.add('active');[m
[32m+[m[32m    document.getElementById(`q${current - 1}`).scrollIntoView({ behavior: 'smooth' });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32masync function submitForm() {[m
[32m+[m[32m    const formData = {[m
[32m+[m[32m        name: document.getElementById('userName').value,[m
[32m+[m[32m        miles_past_320_days: document.getElementById('miles_past_320_days').value,[m
[32m+[m[32m        kilometers_2025: document.getElementById('kilometers_2025').value,[m
[32m+[m[32m        avg_hr_walking: document.getElementById('avg_hr_walking').value,[m
[32m+[m[32m        recent_haircut: document.getElementById('recent_haircut').value,[m
[32m+[m[32m        iron_levels: document.getElementById('iron_levels').value,[m
[32m+[m[32m        last_race_date: document.getElementById('last_race_date').value,[m
[32m+[m[32m        last_date_race: document.getElementById('last_date_race').value,[m
[32m+[m[32m        ssn_digit_sum: document.getElementById('ssn_digit_sum').value[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Hide form, show loading[m
[32m+[m[32m    document.getElementById('questionnaireForm').style.display = 'none';[m
[32m+[m[32m    const loadingSection = document.getElementById('loadingSection');[m
[32m+[m[32m    loadingSection.style.display = 'block';[m
[32m+[m
[32m+[m[32m    // Fake loading sequence[m
[32m+[m[32m    const progressBar = document.querySelector('.progress-bar');[m
[32m+[m[32m    const statusDiv = document.getElementById('calculationStatus');[m
[32m+[m[32m    const statuses = [[m
[32m+[m[32m        "Analyzing track conditions...",[m
[32m+[m[32m        "Processing community insights...",[m
[32m+[m[32m        "Calibrating workout parameters...",[m
[32m+[m[32m        "Validating with track veterans...",[m
[32m+[m[32m        "Optimizing for 320m segments...",[m
[32m+[m[32m        "Fine-tuning intensity levels...",[m
[32m+[m[32m        "Preparing your custom session..."[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    for (let i = 0; i < statuses.length; i++) {[m
[32m+[m[32m        statusDiv.textContent = statuses[i];[m
[32m+[m[32m        progressBar.style.width = `${(i + 1) * (100 / statuses.length)}%`;[m
[32m+[m[32m        await new Promise(r => setTimeout(r, 800));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m        const response = await fetch('/generate_workout', {[m
[32m+[m[32m            method: 'POST',[m
[32m+[m[32m            headers: {[m
[32m+[m[32m                'Content-Type': 'application/json'[m
[32m+[m[32m            },[m
[32m+[m[32m            body: JSON.stringify(formData)[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        const data = await response.json();[m
[32m+[m
[32m+[m[32m        // Hide loading, show results[m
[32m+[m[32m        loadingSection.style.display = 'none';[m
[32m+[m[32m        const resultDiv = document.getElementById('workoutResult');[m
[32m+[m[32m        resultDiv.style.display = 'block';[m
[32m+[m
[32m+[m[32m        resultDiv.innerHTML = `[m
[32m+[m[32m            <div class="alert" style="background: rgba(0, 0, 0, 0.7); border: 1px solid rgba(52, 152, 219, 0.3); border-radius: 12px; color: white;">[m
[32m+[m[32m                <div class="track-badge mb-3">SESSION ${Math.floor(Math.random() * 900000) + 100000}</div>[m
[32m+[m[32m                <h4 class="alert-heading mb-4">${data.workout_intro}</h4>[m
[32m+[m[32m                <div class="workout-text">[m
[32m+[m[32m                    <h3 class="text-gradient mb-3">${data.workout_details.intensity}</h3>[m
[32m+[m[32m                    ${data.workout_details.workout.replace(/\n/g, '<br>')}[m
[32m+[m[32m                </div>[m
[32m+[m[32m                <hr style="border-color: rgba(52, 152, 219, 0.3);">[m
[32m+[m[32m                <p><em>${data.workout_details.science}</em></p>[m
[32m+[m[32m                <p class="mb-0"><small class="text-uppercase" style="letter-spacing: 1px;">${data.ai_insight}</small></p>[m
[32m+[m[32m                <div class="mt-3" style="color: rgba(255, 255, 255, 0.6); font-size: 0.8rem;">[m
[32m+[m[32m                    <span class="text-uppercase">Custom Workout</span> •[m[41m [m
[32m+[m[32m                    Validated by ${data.meta.models_consulted} track club members •[m[41m [m
[32m+[m[32m                    Analysis time: ${data.meta.processing_time_ms}ms[m
[32m+[m[32m                </div>[m
[32m+[m[32m            </div>[m
[32m+[m[32m        `;[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m        console.error('Error:', error);[m
[32m+[m[32m        alert('Error generating workout. Please try again.');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m[32m</script>[m
[32m+[m[32m{% endblock %}[m[41m [m
