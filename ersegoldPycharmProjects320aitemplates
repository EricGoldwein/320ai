[32m+[m[32m@app.route("/subscribe", methods=["POST"])[m
[32m+[m[32mdef subscribe():[m
[32m+[m[32m    """Handle email subscription"""[m
[32m+[m[32m    try:[m
[32m+[m[32m        data = request.json[m
[32m+[m[32m        email = data.get('email')[m
[32m+[m[41m        [m
[32m+[m[32m        if not email:[m
[32m+[m[32m            return jsonify({"success": False, "message": "Email is required"}), 400[m
[32m+[m[41m            [m
[32m+[m[32m        # Connect to database[m
[32m+[m[32m        db_path = os.path.join(app.root_path, 'subscribers.db')[m
[32m+[m[32m        conn = sqlite3.connect(db_path)[m
[32m+[m[32m        c = conn.cursor()[m
[32m+[m[41m        [m
[32m+[m[32m        # Try to insert the email[m
[32m+[m[32m        try:[m
[32m+[m[32m            c.execute('INSERT INTO subscribers (email) VALUES (?)', (email,))[m
[32m+[m[32m            conn.commit()[m
[32m+[m[32m            return jsonify({[m
[32m+[m[32m                "success": True,[m
[32m+[m[32m                "message": "Successfully subscribed!"[m
[32m+[m[32m            })[m
[32m+[m[32m        except sqlite3.IntegrityError:[m
[32m+[m[32m            return jsonify({[m
[32m+[m[32m                "success": False,[m
[32m+[m[32m                "message": "You're already subscribed!"[m
[32m+[m[32m            }), 400[m
[32m+[m[32m        finally:[m
[32m+[m[32m            conn.close()[m
[32m+[m[41m            [m
[32m+[m[32m    except Exception as e:[m
[32m+[m[32m        print(f"Error in subscribe: {str(e)}")[m
[32m+[m[32m        return jsonify({[m
[32m+[m[32m            "success": False,[m
[32m+[m[32m            "message": "Server error. Please try again."[m
[32m+[m[32m        }), 500[m
[32m+[m
[32m+[m
[32m+[m[32mif __name__ == "__main__":[m
[32m+[m[32m    app.run(port=5009, debug=True)[m
