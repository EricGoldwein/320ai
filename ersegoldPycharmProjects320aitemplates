[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Add collectible items array and constants[m
[32m+[m[32mlet collectibles = [];[m
[32m+[m[32mconst COLLECTIBLE_TYPES = {[m
[32m+[m[32m    CREATINE: 'creatine',[m
[32m+[m[32m    CARBON_PLATES: 'carbon-plates'[m
[32m+[m[32m};[m
[32m+[m[32mconst COLLECTIBLE_POINTS = {[m
[32m+[m[32m    [COLLECTIBLE_TYPES.CREATINE]: 25,[m
[32m+[m[32m    [COLLECTIBLE_TYPES.CARBON_PLATES]: 50[m
[32m+[m[32m};[m
[32m+[m[32mconst COLLECTIBLE_SPAWN_CHANCE = 0.01; // 1% chance per frame to spawn a collectible[m
[32m+[m
[32m+[m[32m// Update the game state to include a win state[m
[32m+[m[32mlet gameState = {[m
[32m+[m[32m    running: false,[m
[32m+[m[32m    paused: false,[m
[32m+[m[32m    score: 0,[m
[32m+[m[32m    highScore: 0,[m
[32m+[m[32m    speed: BASE_SPEED,[m
[32m+[m[32m    frameCount: 0,[m
[32m+[m[32m    backgroundOffset: 0,[m
[32m+[m[32m    pauseTime: 0,[m
[32m+[m[32m    hasWon: false,[m
[32m+[m[32m    collectiblesCollected: {[m
[32m+[m[32m        [COLLECTIBLE_TYPES.CREATINE]: 0,[m
[32m+[m[32m        [COLLECTIBLE_TYPES.CARBON_PLATES]: 0[m
[32m+[m[32m    }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Add flying horses array for kick animation[m
[32m+[m[32mlet flyingHorses = [];[m
[32m+[m
[32m+[m[32m// Add invincibility duration constant[m
[32m+[m[32mconst INVINCIBILITY_DURATION = 5000; // 5 seconds of invincibility[m
[32m+[m
[32m+[m[32m// Initialize game[m
[32m+[m[32mfunction init() {[m
[32m+[m[32m    // Create canvas[m
[32m+[m[32m    canvas = document.createElement('canvas');[m
[32m+[m[32m    canvas.width = CANVAS_WIDTH;[m
[32m+[m[32m    canvas.height = CANVAS_HEIGHT;[m
[32m+[m[32m    ctx = canvas.getContext('2d');[m
[32m+[m[32m    document.getElementById('game-canvas').appendChild(canvas);[m
[32m+[m
[32m+[m[32m    // Load images[m
[32m+[m[32m    loadImages().then(() => {[m
[32m+[m[32m        // Initialize ground segments[m
[32m+[m[32m        for (let i = 0; i < CANVAS_WIDTH / 50 + 1; i++) {[m
[32m+[m[32m            ground.segments.push({[m
[32m+[m[32m                x: i * 50,[m
[32m+[m[32m                width: 50,[m
[32m+[m[32m                height: 2[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Event listeners[m
[32m+[m[32m        document.addEventListener('keydown', handleKeyDown);[m
[32m+[m[32m        document.addEventListener('keyup', handleKeyUp);[m
[32m+[m[41m        [m
[32m+[m[32m        // Add touch controls for mobile[m
[32m+[m[32m        canvas.addEventListener('touchstart', handleTouchStart);[m
[32m+[m[32m        canvas.addEventListener('touchend', handleTouchEnd);[m
[32m+[m[41m        [m
[32m+[m[32m        canvas.addEventListener('click', startGame);[m
[32m+[m[41m        [m
[32m+[m[32m        // Pause button - Fix the pause button to properly handle unpausing[m
[32m+[m[32m        const pauseBtn = document.getElementById('pauseBtn');[m
[32m+[m[32m        pauseBtn.addEventListener('click', function(e) {[m
[32m+[m[32m            e.stopPropagation(); // Prevent the click from starting the game[m
[32m+[m[32m            togglePause();[m
[32m+[m[32m            // Update button text based on game state[m
[32m+[m[32m            this.textContent = gameState.paused ? "RESUME" : "PAUSE";[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        // Start game loop[m
[32m+[m[32m        requestAnimationFrame(gameLoop);[m
[32m+[m[32m        showMessage("PRESS ANY KEY OR TAP TO START");[m
[32m+[m[41m        [m
[32m+[m[32m        // Add mobile control buttons[m
[32m+[m[32m        addMobileControls();[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction loadImages() {[m
[32m+[m[32m    return Promise.all([[m
[32m+[m[32m        loadImage('/static/images/game2/horse.png').then(img => ASSETS.horse = img),[m
[32m+[m[32m        loadImage('/static/images/game2/horse-seabiscuit.png').then(img => ASSETS.rival_horse = img),[m
[32m+[m[32m        loadImage('/static/images/game2/Rattle_Snake_Pixel.png').then(img => ASSETS.snake = img),[m
[32m+[m[32m        loadImage('/static/images/game2/grizzlyidlev2.png').then(img => ASSETS.grizzly = img),[m
[32m+[m[32m        loadImage('/static/images/game2/porcupine.png').then(img => ASSETS.porcupine = img)[m
[32m+[m[32m    ]);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction loadImage(src) {[m
[32m+[m[32m    return new Promise((resolve, reject) => {[m
[32m+[m[32m        const img = new Image();[m
[32m+[m[32m        img.onload = () => resolve(img);[m
[32m+[m[32m        img.onerror = reject;[m
[32m+[m[32m        img.src = src;[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction handleKeyDown(event) {[m
[32m+[m[32m    if (!gameState.running) {[m
[32m+[m[32m        startGame();[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Fix the pause/resume functionality with separate keys[m
[32m+[m[32m    if (event.code === 'KeyP' && !gameState.paused) {[m
[32m+[m[32m        togglePause(); // Only use P to pause, not to resume[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    if (event.code === 'KeyR' && gameState.paused) {[m
[32m+[m[32m        togglePause(); // Use R to resume when paused[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Don't process other keys if game is paused[m
[32m+[m[32m    if (gameState.paused) {[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ((event.code === 'ArrowUp' || event.code === 'KeyW')) {[m
[32m+[m[32m        // Allow jump if on ground or double jump if already jumping[m
[32m+[m[32m        if (!daisy.isJumping) {[m
[32m+[m[32m            jump();[m
[32m+[m[32m            daisy.canDoubleJump = true;  // Enable double jump after first jump[m
[32m+[m[32m        } else if (daisy.canDoubleJump) {[m
[32m+[m[32m            // Double jump with reduced force[m
[32m+[m[32m            daisy.velocityY = JUMP_FORCE * 0.7;[m
[32m+[m[32m            daisy.canDoubleJump = false;  // Disable after using double jump[m
[32m+[m[41m            [m
[32m+[m[32m            // Double jump particles (yellow)[m
[32m+[m[32m            createParticleBurst(daisy.x + daisy.width/2, daisy.y + daisy.height/2, 12, '#FFD700');[m
[32m+[m[41m            [m
[32m+[m[32m            // Show double jump message with 320-themed running terminology[m
[32m+[m[32m            showMessage("SECOND WIND! 320 POWER!", 500);[m
[32m+[m[32m        }[m
[32m+[m[32m    } else if (event.code === 'Space') {[m
[32m+[m[32m        kick();[m
[32m+[m[32m    } else if (event.code === 'KeyG') {[m
[32m+[m[32m        buyGel();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction handleKeyUp(event) {[m
[32m+[m[32m    if (event.code === 'Space') {[m
[32m+[m[32m        daisy.isKicking = false;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction togglePause() {[m
[32m+[m[32m    gameState.paused = !gameState.paused;[m
[32m+[m[41m    [m
[32m+[m[32m    // Update button text[m
[32m+[m[32m    document.getElementById('pauseBtn').textContent = gameState.paused ? "RESUME" : "PAUSE";[m
[32m+[m[41m    [m
[32m+[m[32m    if (gameState.paused) {[m
[32m+[m[32m        showMessage("PAUSED - PRESS R TO RESUME");[m
[32m+[m[32m        // Start calf raises when paused, but with less intensity[m
[32m+[m[32m        daisy.doingCalfRaises = true;[m
[32m+[m[32m        gameState.pauseTime = Date.now();[m
[32m+[m[32m    } else {[m
[32m+[m[32m        hideMessage();[m
[32m+[m[32m        // Stop calf raises when unpaused[m
[32m+[m[32m        daisy.doingCalfRaises = false;[m
[32m+[m[32m        daisy.calfRaiseHeight = 0;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction startGame() {[m
[32m+[m[32m    gameState.running = true;[m
[32m+[m[32m    gameState.paused = false;[m
[32m+[m[32m    gameState.score = 0;[m
[32m+[m[32m    gameState.speed = BASE_SPEED;[m
[32m+[m[32m    gameState.hasWon = false;[m
[32m+[m[41m    [m
[32m+[m[32m    daisy.gels = 0;[m
[32m+[m[32m    daisy.hasGelActive = false;[m
[32m+[m[32m    daisy.isInvincible = false;[m
[32m+[m[32m    daisy.invincibilityEndTime = 0;[m
[32m+[m[32m    daisy.doingCalfRaises = false;[m
[32m+[m[32m    daisy.canDoubleJump = false;[m
[32m+[m[32m    daisy.streak = 0;[m
[32m+[m[32m    daisy.multiplier = 1;[m
[32m+[m[41m    [m
[32m+[m[32m    obstacles = [];[m
[32m+[m[32m    particles = [];[m
[32m+[m[32m    collectibles = [];[m
[32m+[m[32m    gameState.collectiblesCollected = {[m
[32m+[m[32m        [COLLECTIBLE_TYPES.CREATINE]: 0,[m
[32m+[m[32m        [COLLECTIBLE_TYPES.CARBON_PLATES]: 0[m
[32m+[m[32m    };[m
[32m+[m[41m    [m
[32m+[m[32m    // Update pause button text[m
[32m+[m[32m    document.getElementById('pauseBtn').textContent = "PAUSE";[m
[32m+[m[41m    [m
[32m+[m[32m    hideMessage();[m
[32m+[m[32m    document.getElementById('gels').textContent = daisy.gels;[m
[32m+[m[41m    [m
[32m+[m[32m    // Add starting particles for visual flair[m
[32m+[m[32m    createParticleBurst(CANVAS_WIDTH / 2, CANVAS_HEIGHT / 2, 20, '#FFD700');[m
[32m+[m[41m    [m
[32m+[m[32m    // Add a few initial collectibles at different positions[m
[32m+[m[32m    addInitialCollectibles();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction addInitialCollectibles() {[m
[32m+[m[32m    // Add 3-5 collectibles at the start[m
[32m+[m[32m    const numCollectibles = Math.floor(Math.random() * 3) + 3;[m
[32m+[m[41m    [m
[32m+[m[32m    for (let i = 0; i < numCollectibles; i++) {[m
[32m+[m[32m        const type = Math.random() > 0.5 ?[m[41m [m
[32m+[m[32m            COLLECTIBLE_TYPES.CREATINE :[m[41m [m
[32m+[m[32m            COLLECTIBLE_TYPES.CARBON_PLATES;[m
[32m+[m[41m        [m
[32m+[m[32m        // Position them at different distances and heights[m
[32m+[m[32m        const xPosition = CANVAS_WIDTH + (i * 300) + Math.random() * 200;[m
[32m+[m[32m        const heightVariation = Math.random() > 0.5 ?[m[41m [m
[32m+[m[32m            Math.random() * 100 : // Higher position (50% chance)[m
[32m+[m[32m            0;                    // Ground level (50% chance)[m
[32m+[m[41m        [m
[32m+[m[32m        collectibles.push({[m
[32m+[m[32m            type: type,[m
[32m+[m[32m            x: xPosition,[m
[32m+[m[32m            y: GROUND_HEIGHT - 30 - heightVariation,[m
[32m+[m[32m            width: 30,[m
[32m+[m[32m            height: 30,[m
[32m+[m[32m            rotation: 0,[m
[32m+[m[32m            bobHeight: 0,[m
[32m+[m[32m            bobSpeed: Math.random() * 0.05 + 0.02[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction buyGel() {[m
[32m+[m[32m    if (gameState.score >= GEL_COST) {[m
[32m+[m[32m        gameState.score -= GEL_COST;[m
[32m+[m[32m        daisy.gels++;[m
[32m+[m[32m        document.getElementById('gels').textContent = daisy.gels;[m
[32m+[m[32m        document.getElementById('wingos').textContent = Math.floor(gameState.score);[m
[32m+[m[32m        showMessage("Bought 1 Gel! Press J for 320 POWER MODE!", 1000);[m
[32m+[m[41m        [m
[32m+[m[32m        // Add particles for visual feedback[m
[32m+[m[32m        createParticleBurst(daisy.x + daisy.width/2, daisy.y, 10, '#00FFFF');[m
[32m+[m[32m    } else {[m
[32m+[m[32m        showMessage("Need " + GEL_COST + " Wingos for Gel!", 1000);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction jump() {[m
[32m+[m[32m    let jumpForce = JUMP_FORCE;[m
[32m+[m[41m    [m
[32m+[m[32m    // Add regular jump particles[m
[32m+[m[32m    createParticleBurst(daisy.x + daisy.width/2, daisy.y + daisy.height, 8, '#8B4513');[m
[32m+[m[41m    [m
[32m+[m[32m    daisy.velocityY = jumpForce;[m
[32m+[m[32m    daisy.isJumping = true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction kick() {[m
[32m+[m[32m    if (daisy.isKicking) return;[m
[32m+[m[41m    [m
[32m+[m[32m    daisy.isKicking = true;[m
[32m+[m[41m    [m
[32m+[m[32m    // Add kick effect particles[m
[32m+[m[32m    createParticleBurst(daisy.x + daisy.width, daisy.y + daisy.height/2, 5, '#FF6347');[m
[32m+[m[41m    [m
[32m+[m[32m    let kickedSomething = false;[m
[32m+[m[41m    [m
[32m+[m[32m    obstacles.forEach((obstacle, index) => {[m
[32m+[m[32m        const kickRange = {[m
[32m+[m[32m            x: daisy.x + daisy.width/2,[m
[32m+[m[32m            y: daisy.y,[m
[32m+[m[32m            width: daisy.width,[m
[32m+[m[32m            height: daisy.height[m
[32m+[m[32m        };[m
[32m+[m[41m        [m
[32m+[m[32m        if (obstacle.type === 'horse' && isColliding(kickRange, obstacle)) {[m
[32m+[m[32m            kickedSomething = true;[m
[32m+[m[41m            [m
[32m+[m[32m            // Add the horse to flying horses array with initial velocity[m
[32m+[m[32m            flyingHorses.push({[m
[32m+[m[32m                x: obstacle.x,[m
[32m+[m[32m                y: obstacle.y,[m
[32m+[m[32m                width: obstacle.width,[m
[32m+[m[32m                height: obstacle.height,[m
[32m+[m[32m                velocityY: KICK_FORCE,[m
[32m+[m[32m                velocityX: 10,[m
[32m+[m[32m                rotation: 0,[m
[32m+[m[32m                rotationSpeed: Math.random() * 0.2 - 0.1  // Random rotation[m
[32m+[m[32m            });[m
[32m+[m[41m            [m
[32m+[m[32m            // Create explosion of particles[m
[32m+[m[32m            createParticleBurst(obstacle.x + obstacle.width/2, obstacle.y, 20, '#FFA500');[m
[32m+[m[41m            [m
[32m+[m[32m            obstacles.splice(index, 1);[m
[32m+[m[41m            [m
[32m+[m[32m            // Increase streak and multiplier with 320-themed running terminology[m
[32m+[m[32m            daisy.streak++;[m
[32m+[m[32m            if (daisy.streak > 1) {[m
[32m+[m[32m                daisy.multiplier = Math.min(4, 1 + (daisy.streak * 0.5));[m
[32m+[m[32m                gameState.score += 10 * daisy.multiplier;[m
[32m+[m[41m                [m
[32m+[m[32m                // Use 320-themed running terminology for combo messages[m
[32m+[m[32m                const comboMessages = [[m
[32m+[m[32m                    `320 COMBO x${daisy.streak}!`,[m
[32m+[m[32m                    `WINGATE POWER x${daisy.streak}!`,[m
[32m+[m[32m                    `320 SPLIT x${daisy.streak}!`,[m
[32m+[m[32m                    `TRACK MASTER x${daisy.streak}!`[m
[32m+[m[32m                ];[m
[32m+[m[32m                const message = comboMessages[Math.min(daisy.streak - 2, comboMessages.length - 1)];[m
[32m+[m[32m                showMessage(`${message} +${Math.floor(10 * daisy.multiplier)} WINGOS!`, 500);[m
[32m+[m[32m            } else {[m
[32m+[m[32m                gameState.score += 10;[m
[32m+[m[32m                showMessage("KICKED! +10 WINGOS!", 500);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Reset streak if nothing was kicked[m
[32m+[m[32m    if (!kickedSomething) {[m
[32m+[m[32m        daisy.streak = 0;[m
[32m+[m[32m        daisy.multiplier = 1;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    setTimeout(() => {[m
[32m+[m[32m        daisy.isKicking = false;[m
[32m+[m[32m    }, 500);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Particle system[m
[32m+[m[32mfunction createParticle(x, y, color) {[m
[32m+[m[32m    return {[m
[32m+[m[32m        x: x,[m
[32m+[m[32m        y: y,[m
[32m+[m[32m        size: Math.random() * 5 + 2,[m
[32m+[m[32m        speedX: Math.random() * 6 - 3,[m
[32m+[m[32m        speedY: Math.random() * 6 - 3,[m
[32m+[m[32m        color: color,[m
[32m+[m[32m        life: Math.random() * 30 + 20[m
[32m+[m[32m    };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction createParticleBurst(x, y, amount, color) {[m
[32m+[m[32m    for (let i = 0; i < amount; i++) {[m
[32m+[m[32m        particles.push(createParticle(x, y, color));[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction updateParticles() {[m
[32m+[m[32m    for (let i = particles.length - 1; i >= 0; i--) {[m
[32m+[m[32m        const p = particles[i];[m
[32m+[m[32m        p.x += p.speedX;[m
[32m+[m[32m        p.y += p.speedY;[m
[32m+[m[41m        [m
[32m+[m[32m        // Apply gravity if this particle has it[m
[32m+[m[32m        if (p.gravity) {[m
[32m+[m[32m            p.speedY += p.gravity;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        p.life--;[m
[32m+[m[41m        [m
[32m+[m[32m        if (p.life <= 0) {[m
[32m+[m[32m            particles.splice(i, 1);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction drawParticles() {[m
[32m+[m[32m    particles.forEach(p => {[m
[32m+[m[32m        ctx.globalAlpha = p.life / 50; // Fade out as life decreases[m
[32m+[m[32m        ctx.fillStyle = p.color;[m
[32m+[m[32m        ctx.beginPath();[m
[32m+[m[32m        ctx.arc(p.x, p.y, p.size, 0, Math.PI * 2);[m
[32m+[m[32m        ctx.fill();[m
[32m+[m[32m    });[m
[32m+[m[32m    ctx.globalAlpha = 1; // Reset alpha[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction gameLoop() {[m
[32m+[m[32m    if (gameState.running && !gameState.paused) {[m
[32m+[m[32m        update();[m
[32m+[m[32m    } else if (gameState.paused) {[m
[32m+[m[32m        // Update calf raises animation during pause[m
[32m+[m[32m        updateCalfRaises();[m
[32m+[m[32m    }[m
[32m+[m[32m    draw();[m
[32m+[m[32m    requestAnimationFrame(gameLoop);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction updateCalfRaises() {[m
[32m+[m[32m    // Calculate calf raise height using sine wave for smooth up/down motion[m
[32m+[m[32m    // Reduced amplitude from 10 to 5 for less intense movement[m
[32m+[m[32m    const elapsed = Date.now() - gameState.pauseTime;[m
[32m+[m[32m    daisy.calfRaiseHeight = Math.sin(elapsed / 500) * 5; // 5px max height, slower cycle[m
[32m+[m[41m    [m
[32m+[m[32m    // Update particles even when paused[m
[32m+[m[32m    updateParticles();[m
[32m+[m[41m    [m
[32m+[m[32m    // Reduce frequency of dust particles during calf raises[m
[32m+[m[32m    if (Math.random() < 0.03) { // Reduced from 0.1 to 0.03[m
[32m+[m[32m        createParticleBurst([m
[32m+[m[32m            daisy.x + daisy.width/2,[m[41m [m
[32m+[m[32m            daisy.y + daisy.height,[m[41m [m
[32m+[m[32m            1, // Reduced from 2 to 1[m
[32m+[m[32m            '#D2B48C'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction update() {[m
[32m+[m[32m    gameState.frameCount++;[m
[32m+[m[32m    daisy.frameCount++;[m
[32m+[m[41m    [m
[32m+[m[32m    // Update background parallax[m
[32m+[m[32m    gameState.backgroundOffset += gameState.speed * 0.2;[m
[32m+[m
[32m+[m[32m    // Update invincibility timer[m
[32m+[m[32m    if (daisy.isInvincible) {[m
[32m+[m[32m        if (Date.now() >= daisy.invincibilityEndTime) {[m
[32m+[m[32m            // End invincibility[m
[32m+[m[32m            daisy.isInvincible = false;[m
[32m+[m[32m            showMessage("320 POWER MODE ENDED", 1000);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            // Create trailing particles while invincible[m
[32m+[m[32m            if (gameState.frameCount % 2 === 0) {[m
[32m+[m[32m                createParticleBurst([m
[32m+[m[32m                    daisy.x + Math.random() * daisy.width,[m[41m [m
[32m+[m[32m                    daisy.y + Math.random() * daisy.height,[m[41m [m
[32m+[m[32m                    1,[m[41m [m
[32m+[m[32m                    `hsl(${(gameState.frameCount * 5) % 360}, 100%, 50%)`  // Rainbow colors[m
[32m+[m[32m                );[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Update Daisy[m
[32m+[m[32m    if (daisy.isJumping) {[m
[32m+[m[32m        daisy.velocityY += GRAVITY;[m
[32m+[m[32m        daisy.y += daisy.velocityY;[m
[32m+[m
[32m+[m[32m        if (daisy.y >= GROUND_HEIGHT - 50) {[m
[32m+[m[32m            daisy.y = GROUND_HEIGHT - 50;[m
[32m+[m[32m            daisy.velocityY = 0;[m
[32m+[m[32m            daisy.isJumping = false;[m
[32m+[m[32m            daisy.canDoubleJump = false;  // Reset double jump on landing[m
[32m+[m[41m            [m
[32m+[m[32m            // Landing particles[m
[32m+[m[32m            createParticleBurst(daisy.x + daisy.width/2, daisy.y + daisy.height, 5, '#8B4513');[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Animate Daisy - smoother animation[m
[32m+[m[32m    if (daisy.frameCount % ANIMATION_SPEED === 0) {[m
[32m+[m[32m        daisy.frame = (daisy.frame + 1) % ANIMATION_FRAMES;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Random chaos (speed changes) - REMOVED to prevent abrupt pauses[m
[32m+[m[32m    // Keep speed consistent[m
[32m+[m[32m    gameState.speed = BASE_SPEED;[m
[32m+[m
[32m+[m[32m    // Update obstacles - spawn more frequently to make game harder[m
[32m+[m[32m    if (gameState.frameCount % 80 === 0) {  // Reduced from 100 to 80[m
[32m+[m[32m        spawnObstacle();[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Try to spawn collectibles - make them rarer[m
[32m+[m[32m    if (Math.random() < COLLECTIBLE_SPAWN_CHANCE * 0.5) {  // Reduced chance by 50%[m
[32m+[m[32m        spawnCollectible();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    obstacles.forEach((obstacle, index) => {[m
[32m+[m[32m        obstacle.x -= gameState.speed;[m
[32m+[m[41m        [m
[32m+[m[32m        // Remove off-screen obstacles[m
[32m+[m[32m        if (obstacle.x + obstacle.width < 0) {[m
[32m+[m[32m            obstacles.splice(index, 1);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Check collisions - skip if invincible[m
[32m+[m[32m        if (!daisy.isKicking && !daisy.isInvincible && isColliding(daisy, obstacle)) {[m
[32m+[m[32m            if (obstacle.type === 'horse' && daisy.isKicking) {[m
[32m+[m[32m                obstacles.splice(index, 1);[m
[32m+[m[32m                gameState.score += 10;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                gameOver();[m
[32m+[m[32m            }[m
[32m+[m[32m        } else if (daisy.isInvincible && isColliding(daisy, obstacle)) {[m
[32m+[m[32m            // When invincible, destroy obstacles on contact and gain points[m
[32m+[m[32m            createParticleBurst(obstacle.x + obstacle.width/2, obstacle.y, 20, '#00FFFF');[m
[32m+[m[32m            obstacles.splice(index, 1);[m
[32m+[m[32m            gameState.score += 5;[m
[32m+[m[32m            showMessage("+5 WINGOS!", 300);[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Update collectibles[m
[32m+[m[32m    collectibles.forEach((collectible, index) => {[m
[32m+[m[32m        collectible.x -= gameState.speed;[m
[32m+[m[41m        [m
[32m+[m[32m        // Bobbing animation[m
[32m+[m[32m        collectible.bobHeight = Math.sin(gameState.frameCount * collectible.bobSpeed) * 5;[m
[32m+[m[32m        collectible.rotation += 0.02;[m
[32m+[m[41m        [m
[32m+[m[32m        // Remove off-screen collectibles[m
[32m+[m[32m        if (collectible.x + collectible.width < 0) {[m
[32m+[m[32m            collectibles.splice(index, 1);[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Check for collection[m
[32m+[m[32m        if (isColliding(daisy, collectible)) {[m
[32m+[m[32m            // Collect the item[m
[32m+[m[32m            collectibles.splice(index, 1);[m
[32m+[m[41m            [m
[32m+[m[32m            // Add points based on type[m
[32m+[m[32m            const points = COLLECTIBLE_POINTS[collectible.type];[m
[32m+[m[32m            gameState.score += points;[m
[32m+[m[41m            [m
[32m+[m[32m            // Track collection[m
[32m+[m[32m            gameState.collectiblesCollected[collectible.type]++;[m
[32m+[m[41m            [m
[32m+[m[32m            // Create special effect based on type[m
[32m+[m[32m            if (collectible.type === COLLECTIBLE_TYPES.CREATINE) {[m
[32m+[m[32m                // Creatine effect - red/purple burst[m
[32m+[m[32m                createParticleBurst([m
[32m+[m[32m                    collectible.x + collectible.width/2,[m[41m [m
[32m+[m[32m                    collectible.y,[m[41m [m
[32m+[m[32m                    30,[m[41m [m
[32m+[m[32m                    '#FF00FF'[m
[32m+[m[32m                );[m
[32m+[m[32m                showMessage(`CREATINE POWER! +${points} WINGOS!`, 1000);[m
[32m+[m[41m                [m
[32m+[m[32m                // REMOVED speed boost (turbo feature)[m
[32m+[m[32m            } else if (collectible.type === COLLECTIBLE_TYPES.CARBON_PLATES) {[m
[32m+[m[32m                // Carbon plates effect - green/blue burst[m
[32m+[m[32m                createParticleBurst([m
[32m+[m[32m                    collectible.x + collectible.width/2,[m[41m [m
[32m+[m[32m                    collectible.y,[m[41m [m
[32m+[m[32m                    30,[m[41m [m
[32m+[m[32m                    '#00FFAA'[m
[32m+[m[32m                );[m
[32m+[m[32m                showMessage(`CARBON PLATES! +${points} WINGOS!`, 1000);[m
[32m+[m[41m                [m
[32m+[m[32m                // REMOVED multiplier boost (turbo feature)[m
[32m+[m[32m            }[m
[32m+[m[41m            [m
[32m+[m[32m            // Update score display[m
[32m+[m[32m            document.getElementById('wingos').textContent = Math.floor(gameState.score);[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Update ground[m
[32m+[m[32m    ground.segments.forEach(segment => {[m
[32m+[m[32m        segment.x -= gameState.speed;[m
[32m+[m[32m        if (segment.x + segment.width < 0) {[m
[32m+[m[32m            segment.x = CANVAS_WIDTH;[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Update flying horses[m
[32m+[m[32m    flyingHorses.forEach((horse, index) => {[m
[32m+[m[32m        horse.x += horse.velocityX;[m
[32m+[m[32m        horse.y += horse.velocityY;[m
[32m+[m[32m        horse.velocityY += GRAVITY * 0.5;  // Slower fall for more dramatic effect[m
[32m+[m[32m        horse.rotation += horse.rotationSpeed;[m
[32m+[m[41m        [m
[32m+[m[32m        // Add trail particles[m
[32m+[m[32m        if (gameState.frameCount % 3 === 0) {[m
[32m+[m[32m            particles.push(createParticle([m
[32m+[m[32m                horse.x + horse.width/2,[m[41m [m
[32m+[m[32m                horse.y + horse.height/2,[m
[32m+[m[32m                '#FFA07A'[m
[32m+[m[32m            ));[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Remove if off screen[m
[32m+[m[32m        if (horse.x > CANVAS_WIDTH || horse.y > CANVAS_HEIGHT) {[m
[32m+[m[32m            flyingHorses.splice(index, 1);[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Update particles[m
[32m+[m[32m    updateParticles();[m
[32m+[m
[32m+[m[32m    // Update score - make it harder to earn points[m
[32m+[m[32m    gameState.score += 0.05 * daisy.multiplier;  // Reduced from 0.1 to 0.05[m
[32m+[m[32m    document.getElementById('wingos').textContent = Math.floor(gameState.score);[m
[32m+[m[41m    [m
[32m+[m[32m    // Check for win condition - 320 WINGOS[m
[32m+[m[32m    if (Math.floor(gameState.score) >= 320 && !gameState.hasWon) {[m
[32m+[m[32m        gameState.hasWon = true;[m
[32m+[m[32m        winGame();[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Decay multiplier over time - faster decay to make it harder[m
[32m+[m[32m    if (daisy.multiplier > 1 && gameState.frameCount % 200 === 0) {  // Reduced from 300 to 200[m
[32m+[m[32m        daisy.multiplier = Math.max(1, daisy.multiplier - 0.5);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction draw() {[m
[32m+[m[32m    // Clear canvas with transparent background to show the GIF[m
[32m+[m[32m    ctx.clearRect(0, 0, CANVAS_WIDTH, CANVAS_HEIGHT);[m
[32m+[m[41m    [m
[32m+[m[32m    // Draw invisible ground (no visible line)[m
[32m+[m[32m    ctx.fillStyle = 'rgba(0, 0, 0, 0)';  // Completely transparent[m
[32m+[m[32m    ground.segments.forEach(segment => {[m
[32m+[m[32m        ctx.fillRect(segment.x, ground.y, segment.width, segment.height);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Draw particles behind Daisy[m
[32m+[m[32m    drawParticles();[m
[32m+[m[41m    [m
[32m+[m[32m    // Draw collectibles[m
[32m+[m[32m    collectibles.forEach(collectible => {[m
[32m+[m[32m        ctx.save();[m
[32m+[m[41m        [m
[32m+[m[32m        // Apply bobbing and rotation[m
[32m+[m[32m        ctx.translate([m
[32m+[m[32m            collectible.x + collectible.width/2,[m[41m [m
[32m+[m[32m            collectible.y + collectible.bobHeight + collectible.height/2[m
[32m+[m[32m        );[m
[32m+[m[32m        ctx.rotate(collectible.rotation);[m
[32m+[m[41m        [m
[32m+[m[32m        // Add glow effect[m
[32m+[m[32m        ctx.shadowColor = collectible.type === COLLECTIBLE_TYPES.CREATINE ?[m[41m [m
[32m+[m[32m            '#FF00FF' : '#00FFAA';[m
[32m+[m[32m        ctx.shadowBlur = 10;[m
[32m+[m[41m        [m
[32m+[m[32m        // Draw the collectible[m
[32m+[m[32m        if (collectible.type === COLLECTIBLE_TYPES.CREATINE) {[m
[32m+[m[32m            // Draw creatine (purple container)[m
[32m+[m[32m            ctx.fillStyle = '#8A2BE2';[m
[32m+[m[32m            ctx.fillRect(-collectible.width/2, -collectible.height/2, collectible.width, collectible.height);[m
[32m+[m[41m            [m
[32m+[m[32m            // Label[m
[32m+[m[32m            ctx.fillStyle = 'white';[m
[32m+[m[32m            ctx.font = '8px Arial';[m
[32m+[m[32m            ctx.textAlign = 'center';[m
[32m+[m[32m            ctx.fillText('CR', 0, 3);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            // Draw carbon plates (green shoe)[m
[32m+[m[32m            ctx.fillStyle = '#32CD32';[m
[32m+[m[41m            [m
[32m+[m[32m            // Shoe shape[m
[32m+[m[32m            ctx.beginPath();[m
[32m+[m[32m            ctx.ellipse(0, 0, collectible.width/2, collectible.height/3, 0, 0, Math.PI * 2);[m
[32m+[m[32m            ctx.fill();[m
[32m+[m[41m            [m
[32m+[m[32m            // Sole[m
[32m+[m[32m            ctx.fillStyle = '#00FFAA';[m
[32m+[m[32m            ctx.fillRect(-collectible.width/2, collectible.height/4, collectible.width, collectible.height/6);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        ctx.restore();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Draw obstacles with shadows - rival horses more visible[m
[32m+[m[32m    obstacles.forEach(obstacle => {[m
[32m+[m[32m        let sprite = null;[m
[32m+[m[32m        switch(obstacle.type) {[m
[32m+[m[32m            case 'porcupine':[m
[32m+[m[32m                sprite = ASSETS.porcupine;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'snake':[m
[32m+[m[32m                sprite = ASSETS.snake;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'horse':[m
[32m+[m[32m                // Use Daisy's sprite for rival horses[m
[32m+[m[32m                sprite = ASSETS.horse;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'grizzly':[m
[32m+[m[32m                sprite = ASSETS.grizzly;[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        if (sprite) {[m
[32m+[m[32m            ctx.save();[m
[32m+[m[41m            [m
[32m+[m[32m            if (obstacle.type === 'horse') {[m
[32m+[m[32m                // Make rival horses more visible with a subtle glow[m
[32m+[m[32m                ctx.shadowColor = 'rgba(255, 0, 0, 0.7)';[m
[32m+[m[32m                ctx.shadowBlur = 15;[m
[32m+[m[32m                ctx.shadowOffsetX = 3;[m
[32m+[m[32m                ctx.shadowOffsetY = 3;[m
[32m+[m[41m                [m
[32m+[m[32m                // Draw the rival horse[m
[32m+[m[32m                ctx.translate(obstacle.x + obstacle.width/2, obstacle.y);[m
[32m+[m[41m                [m
[32m+[m[32m                // Add slight bouncing effect - use a consistent value based on position[m
[32m+[m[32m                // This prevents the shaking effect by making the bounce consistent for each horse[m
[32m+[m[32m                const bounceOffset = Math.sin((obstacle.x * 0.01) + (gameState.frameCount * 0.05)) * 2;[m
[32m+[m[32m                ctx.translate(0, bounceOffset);[m
[32m+[m[41m                [m
[32m+[m[32m                // Flip horizontally to face Daisy[m
[32m+[m[32m                ctx.scale(-1, 1);[m
[32m+[m[41m                [m
[32m+[m[32m                // Draw the horse without any overlay[m
[32m+[m[32m                ctx.drawImage([m
[32m+[m[32m                    sprite,[m
[32m+[m[32m                    -obstacle.width/2,[m
[32m+[m[32m                    -obstacle.height,[m
[32m+[m[32m                    obstacle.width,[m
[32m+[m[32m                    obstacle.height[m
[32m+[m[32m                );[m
[32m+[m[41m                [m
[32m+[m[32m                // Add "RIVAL" text above horse[m
[32m+[m[32m                ctx.fillStyle = 'white';[m
[32m+[m[32m                ctx.font = 'bold 10px Arial';[m
[32m+[m[32m                ctx.textAlign = 'center';[m
[32m+[m[32m                ctx.fillText('RIVAL', 0, -obstacle.height - 5);[m
[32m+[m[41m                [m
[32m+[m[32m                ctx.restore();[m
[32m+[m[32m            } else {[m
[32m+[m[32m                // Regular drawing for other obstacles[m
[32m+[m[32m                ctx.shadowColor = 'rgba(0, 0, 0, 0.3)';[m
[32m+[m[32m                ctx.shadowBlur = 5;[m
[32m+[m[32m                ctx.shadowOffsetX = 3;[m
[32m+[m[32m                ctx.shadowOffsetY = 3;[m
[32m+[m[41m                [m
[32m+[m[32m                ctx.drawImage([m
[32m+[m[32m                    sprite,[m
[32m+[m[32m                    obstacle.x,[m
[32m+[m[32m                    obstacle.y - obstacle.height/2,[m
[32m+[m[32m                    obstacle.width,[m
[32m+[m[32m                    obstacle.height[m
[32m+[m[32m                );[m
[32m+[m[32m                ctx.restore();[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Draw Daisy with shadow[m
[32m+[m[32m    if (ASSETS.horse) {[m
[32m+[m[32m        ctx.save();[m
[32m+[m[41m        [m
[32m+[m[32m        // Add shadow[m
[32m+[m[32m        ctx.shadowColor = 'rgba(0, 0, 0, 0.3)';[m
[32m+[m[32m        ctx.shadowBlur = 10;[m
[32m+[m[32m        ctx.shadowOffsetX = 5;[m
[32m+[m[32m        ctx.shadowOffsetY = 5;[m
[32m+[m[41m        [m
[32m+[m[32m        // Apply calf raise height adjustment when paused[m
[32m+[m[32m        let yPosition = daisy.y;[m
[32m+[m[32m        if (daisy.doingCalfRaises) {[m
[32m+[m[32m            yPosition -= daisy.calfRaiseHeight;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        ctx.translate(daisy.x + daisy.width/2, yPosition + daisy.height/2);[m
[32m+[m[41m        [m
[32m+[m[32m        // Add bouncing effect (only when not doing calf raises)[m
[32m+[m[32m        if (!daisy.doingCalfRaises) {[m
[32m+[m[32m            const bounce = Math.sin(daisy.frameCount * 0.1) * 2;[m
[32m+[m[32m            ctx.translate(0, bounce);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Kicking effect[m
[32m+[m[32m        if (daisy.isKicking) {[m
[32m+[m[32m            ctx.scale(1.2, 1);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Invincibility effect - pulsing rainbow glow[m
[32m+[m[32m        if (daisy.isInvincible) {[m
[32m+[m[32m            const timeLeft = daisy.invincibilityEndTime - Date.now();[m
[32m+[m[32m            const pulseRate = Math.sin(Date.now() / 100) * 0.2 + 0.8; // Pulsing between 0.6 and 1.0[m
[32m+[m[41m            [m
[32m+[m[32m            // Rainbow shadow effect[m
[32m+[m[32m            const hue = (Date.now() / 20) % 360;[m
[32m+[m[32m            ctx.shadowColor = `hsl(${hue}, 100%, 50%)`;[m
[32m+[m[32m            ctx.shadowBlur = 20;[m
[32m+[m[41m            [m
[32m+[m[32m            // Pulse size based on time left[m
[32m+[m[32m            const scale = 1 + (pulseRate * 0.2);[m
[32m+[m[32m            ctx.scale(scale, scale);[m
[32m+[m[41m            [m
[32m+[m[32m            // Add timer indicator - improved circle[m
[32m+[m[32m            const timerPercent = timeLeft / INVINCIBILITY_DURATION;[m
[32m+[m[41m            [m
[32m+[m[32m            // Draw outer glow circle[m
[32m+[m[32m            ctx.beginPath();[m
[32m+[m[32m            ctx.arc(0, 0, daisy.width/2 + 15, 0, Math.PI * 2);[m
[32m+[m[32m            ctx.lineWidth = 8;[m
[32m+[m[32m            ctx.strokeStyle = `hsla(${hue}, 100%, 70%, ${pulseRate * 0.3})`;[m
[32m+[m[32m            ctx.stroke();[m
[32m+[m[41m            [m
[32m+[m[32m            // Draw main timer circle[m
[32m+[m[32m            ctx.beginPath();[m
[32m+[m[32m            ctx.arc(0, 0, daisy.width/2 + 15, 0, Math.PI * 2 * timerPercent);[m
[32m+[m[32m            ctx.lineWidth = 5;[m
[32m+[m[32m            ctx.strokeStyle = `hsla(${hue}, 100%, 60%, ${pulseRate * 0.8})`;[m
[32m+[m[32m            ctx.stroke();[m
[32m+[m[41m            [m
[32m+[m[32m            // Draw small sparkles around the circle[m
[32m+[m[32m            const sparkleCount = 8;[m
[32m+[m[32m            for (let i = 0; i < sparkleCount; i++) {[m
[32m+[m[32m                const sparkleAngle = (i / sparkleCount) * Math.PI * 2;[m
[32m+[m[32m                const sparkleX = Math.cos(sparkleAngle + (Date.now() / 500)) * (daisy.width/2 + 15);[m
[32m+[m[32m                const sparkleY = Math.sin(sparkleAngle + (Date.now() / 500)) * (daisy.width/2 + 15);[m
[32m+[m[41m                [m
[32m+[m[32m                ctx.beginPath();[m
[32m+[m[32m                ctx.arc(sparkleX, sparkleY, 3 * pulseRate, 0, Math.PI * 2);[m
[32m+[m[32m                ctx.fillStyle = `hsla(${(hue + i * 30) % 360}, 100%, 70%, ${pulseRate})`;[m
[32m+[m[32m                ctx.fill();[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Double jump available indicator[m
[32m+[m[32m        if (daisy.isJumping && daisy.canDoubleJump) {[m
[32m+[m[32m            ctx.shadowColor = '#FFD700';[m
[32m+[m[32m            ctx.shadowBlur = 15;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        ctx.drawImage([m
[32m+[m[32m            ASSETS.horse,[m
[32m+[m[32m            -daisy.width/2,[m
[32m+[m[32m            -daisy.height/2,[m
[32m+[m[32m            daisy.width,[m
[32m+[m[32m            daisy.height[m
[32m+[m[32m        );[m
[32m+[m[32m        ctx.restore();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Draw flying horses with motion blur effect - remove red tint[m
[32m+[m[32m    flyingHorses.forEach(horse => {[m
[32m+[m[32m        ctx.save();[m
[32m+[m[41m        [m
[32m+[m[32m        // Motion blur effect[m
[32m+[m[32m        ctx.shadowColor = 'rgba(255, 0, 0, 0.6)';[m
[32m+[m[32m        ctx.shadowBlur = 15;[m
[32m+[m[32m        ctx.shadowOffsetX = -5;[m
[32m+[m[32m        ctx.shadowOffsetY = 0;[m
[32m+[m[41m        [m
[32m+[m[32m        ctx.translate(horse.x + horse.width/2, horse.y + horse.height/2);[m
[32m+[m[32m        ctx.rotate(horse.rotation);[m
[32m+[m[41m        [m
[32m+[m[32m        // Use Daisy's sprite for flying horses too[m
[32m+[m[32m        ctx.drawImage([m
[32m+[m[32m            ASSETS.horse,[m
[32m+[m[32m            -horse.width/2,[m
[32m+[m[32m            -horse.height/2,[m
[32m+[m[32m            horse.width,[m
[32m+[m[32m            horse.height[m
[32m+[m[32m        );[m
[32m+[m[41m        [m
[32m+[m[32m        ctx.restore();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Draw multiplier if active with 320 theme[m
[32m+[m[32m    if (daisy.multiplier > 1) {[m
[32m+[m[32m        ctx.save();[m
[32m+[m[32m        ctx.font = '16px "Press Start 2P", monospace';[m
[32m+[m[32m        ctx.fillStyle = '#FFD700';[m
[32m+[m[32m        ctx.textAlign = 'center';[m
[32m+[m[32m        ctx.shadowColor = 'rgba(0, 0, 0, 0.5)';[m
[32m+[m[32m        ctx.shadowBlur = 5;[m
[32m+[m[32m        ctx.fillText(`320 PACE x${daisy.multiplier.toFixed(1)}`, daisy.x + daisy.width/2, daisy.y - 20);[m
[32m+[m[32m        ctx.restore();[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Draw invincibility timer if active[m
[32m+[m[32m    if (daisy.isInvincible) {[m
[32m+[m[32m        const timeLeft = Math.ceil((daisy.invincibilityEndTime - Date.now()) / 1000);[m
[32m+[m[32m        ctx.save();[m
[32m+[m[41m        [m
[32m+[m[32m        // Create a more visible background for the timer[m
[32m+[m[32m        ctx.fillStyle = 'rgba(0, 0, 0, 0.5)';[m
[32m+[m[32m        ctx.roundRect = function(x, y, width, height, radius) {[m
[32m+[m[32m            this.beginPath();[m
[32m+[m[32m            this.moveTo(x + radius, y);[m
[32m+[m[32m            this.arcTo(x + width, y, x + width, y + height, radius);[m
[32m+[m[32m            this.arcTo(x + width, y + height, x, y + height, radius);[m
[32m+[m[32m            this.arcTo(x, y + height, x, y, radius);[m
[32m+[m[32m            this.arcTo(x, y, x + width, y, radius);[m
[32m+[m[32m            this.closePath();[m
[32m+[m[32m            return this;[m
[32m+[m[32m        };[m
[32m+[m[41m        [m
[32m+[m[32m        // Draw rounded rectangle background[m
[32m+[m[32m        const timerText = `320 POWER: ${timeLeft}s`;[m
[32m+[m[32m        const textWidth = timerText.length * 16;[m
[32m+[m[32m        ctx.roundRect(CANVAS_WIDTH/2 - textWidth/2 - 10, 10, textWidth + 20, 30, 10).fill();[m
[32m+[m[41m        [m
[32m+[m[32m        // Draw text with glow[m
[32m+[m[32m        const hue = (Date.now() / 20) % 360;[m
[32m+[m[32m        ctx.font = 'bold 16px "Press Start 2P", monospace';[m
[32m+[m[32m        ctx.textAlign = 'center';[m
[32m+[m[32m        ctx.shadowColor = `hsl(${hue}, 100%, 70%)`;[m
[32m+[m[32m        ctx.shadowBlur = 10;[m
[32m+[m[32m        ctx.fillStyle = '#FFFFFF';[m
[32m+[m[32m        ctx.fillText(timerText, CANVAS_WIDTH/2, 30);[m
[32m+[m[41m        [m
[32m+[m[32m        // Add small animated icons on either side of the text[m
[32m+[m[32m        const iconSize = 12;[m
[32m+[m[32m        const iconOffset = textWidth/2 + 20;[m
[32m+[m[41m        [m
[32m+[m[32m        // Left lightning bolt[m
[32m+[m[32m        ctx.font = `${iconSize}px Arial`;[m
[32m+[m[32m        ctx.fillText('⚡', CANVAS_WIDTH/2 - iconOffset + Math.sin(Date.now() / 200) * 3, 30);[m
[32m+[m[41m        [m
[32m+[m[32m        // Right lightning bolt[m
[32m+[m[32m        ctx.fillText('⚡', CANVAS_WIDTH/2 + iconOffset + Math.sin(Date.now() / 200 + Math.PI) * 3, 30);[m
[32m+[m[41m        [m
[32m+[m[32m        ctx.restore();[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    // Draw collectibles counter in stats[m
[32m+[m[32m    if (gameState.collectiblesCollected[COLLECTIBLE_TYPES.CREATINE] > 0 ||[m[41m [m
[32m+[m[32m        gameState.collectiblesCollected[COLLECTIBLE_TYPES.CARBON_PLATES] > 0) {[m
[32m+[m[41m        [m
[32m+[m[32m        ctx.save();[m
[32m+[m[32m        ctx.font = '12px "Press Start 2P", monospace';[m
[32m+[m[32m        ctx.fillStyle = '#FF00FF';[m
[32m+[m[32m        ctx.textAlign = 'right';[m
[32m+[m[32m        ctx.shadowColor = 'rgba(0, 0, 0, 0.5)';[m
[32m+[m[32m        ctx.shadowBlur = 5;[m
[32m+[m[41m        [m
[32m+[m[32m        // Position in top right corner below other stats[m
[32m+[m[32m        const statsX = CANVAS_WIDTH - 15;[m
[32m+[m[32m        let statsY = 100;[m
[32m+[m[41m        [m
[32m+[m[32m        if (gameState.collectiblesCollected[COLLECTIBLE_TYPES.CREATINE] > 0) {[m
[32m+[m[32m            ctx.fillText(`CR: ${gameState.collectiblesCollected[COLLECTIBLE_TYPES.CREATINE]}`, statsX, statsY);[m
[32m+[m[32m            statsY += 20;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        if (gameState.collectiblesCollected[COLLECTIBLE_TYPES.CARBON_PLATES] > 0) {[m
[32m+[m[32m            ctx.fillStyle = '#00FFAA';[m
[32m+[m[32m            ctx.fillText(`CP: ${gameState.collectiblesCollected[COLLECTIBLE_TYPES.CARBON_PLATES]}`, statsX, statsY);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        ctx.restore();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Draw western-themed pause overlay - less intrusive[m
[32m+[m[32m    if (gameState.paused) {[m
[32m+[m[32m        // Draw semi-transparent overlay - reduced opacity[m
[32m+[m[32m        ctx.fillStyle = 'rgba(0, 0, 0, 0.2)'; // Reduced from 0.3 to 0.2[m
[32m+[m[32m        ctx.fillRect(0, 0, CANVAS_WIDTH, CANVAS_HEIGHT);[m
[32m+[m[41m        [m
[32m+[m[32m        // Draw western-themed pause indicator[m
[32m+[m[32m        ctx.save();[m
[32m+[m[32m        ctx.font = '16px "Press Start 2P", monospace'; // Reduced from 20px to 16px[m
[32m+[m[32m        ctx.fillStyle = '#FFD700';[m
[32m+[m[32m        ctx.textAlign = 'center';[m
[32m+[m[32m        ctx.shadowColor = 'rgba(0, 0, 0, 0.5)';[m
[32m+[m[32m        ctx.shadowBlur = 5;[m
[32m+[m[32m        ctx.shadowOffsetX = 2;[m
[32m+[m[32m        ctx.shadowOffsetY = 2;[m
[32m+[m[41m        [m
[32m+[m[32m        // Draw cowboy hat above "PAUSED" text - smaller[m
[32m+[m[32m        ctx.font = '24px serif'; // Reduced from 30px to 24px[m
[32m+[m[32m        ctx.fillText('🤠', CANVAS_WIDTH/2, CANVAS_HEIGHT/2 - 30); // Moved up slightly[m
[32m+[m[41m        [m
[32m+[m[32m        // Draw "PAUSED" text - changed to be more subtle[m
[32m+[m[32m        ctx.font = '16px "Press Start 2P", monospace'; // Reduced from 20px to 16px[m
[32m+[m[32m        ctx.fillText('PRESS R TO RESUME', CANVAS_WIDTH/2, CANVAS_HEIGHT/2 + 30); // Changed text[m
[32m+[m[32m        ctx.restore();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Draw win message if player has won[m
[32m+[m[32m    if (gameState.hasWon && !gameState.running) {[m
[32m+[m[32m        ctx.save();[m
[32m+[m[41m        [m
[32m+[m[32m        // Draw semi-transparent overlay[m
[32m+[m[32m        ctx.fillStyle = 'rgba(0, 0, 0, 0.3)';[m
[32m+[m[32m        ctx.fillRect(0, 0, CANVAS_WIDTH, CANVAS_HEIGHT);[m
[32m+[m[41m        [m
[32m+[m[32m        // Draw win text[m
[32m+[m[32m        ctx.font = 'bold 24px "Press Start 2P", monospace';[m
[32m+[m[32m        ctx.textAlign = 'center';[m
[32m+[m[32m        ctx.fillStyle = '#FFD700'; // Gold color[m
[32m+[m[32m        ctx.shadowColor = 'rgba(0, 0, 0, 0.5)';[m
[32m+[m[32m        ctx.shadowBlur = 10;[m
[32m+[m[32m        ctx.fillText('320 WINGOS!', CANVAS_WIDTH/2, CANVAS_HEIGHT/2 - 30);[m
[32m+[m[32m        ctx.fillText('YOU WIN!', CANVAS_WIDTH/2, CANVAS_HEIGHT/2 + 10);[m
[32m+[m[41m        [m
[32m+[m[32m        // Draw smaller instruction text[m
[32m+[m[32m        ctx.font = '12px "Press Start 2P", monospace';[m
[32m+[m[32m        ctx.fillText('PRESS ANY KEY TO PLAY AGAIN', CANVAS_WIDTH/2, CANVAS_HEIGHT/2 + 50);[m
[32m+[m[41m        [m
[32m+[m[32m        ctx.restore();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction spawnObstacle() {[m
[32m+[m[32m    // Increase frequency of horses to make them more visible[m
[32m+[m[32m    const types = ['snake', 'horse', 'porcupine', 'horse', 'horse', 'grizzly']; // Added extra 'horse'[m
[32m+[m[32m    const type = types[Math.floor(Math.random() * types.length)];[m
[32m+[m[41m    [m
[32m+[m[32m    let obstacle = {[m
[32m+[m[32m        type: type,[m
[32m+[m[32m        x: CANVAS_WIDTH,[m
[32m+[m[32m        y: GROUND_HEIGHT,[m
[32m+[m[32m        width: 50,[m
[32m+[m[32m        height: 50[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    switch(type) {[m
[32m+[m[32m        case 'porcupine':[m
[32m+[m[32m            obstacle.width = 40;[m
[32m+[m[32m            obstacle.height = 30;[m
[32m+[m[32m            break;[m
[32m+[m[32m        case 'snake':[m
[32m+[m[32m            obstacle.width = 40;[m
[32m+[m[32m            obstacle.height = 20;[m
[32m+[m[32m            break;[m
[32m+[m[32m        case 'horse':[m
[32m+[m[32m            // Make rival horses a bit larger to be more visible[m
[32m+[m[32m            obstacle.width = 60;[m
[32m+[m[32m            obstacle.height = 60;[m
[32m+[m[32m            // Place them exactly at ground level[m
[32m+[m[32m            obstacle.y = GROUND_HEIGHT;[m
[32m+[m[32m            break;[m
[32m+[m[32m        case 'grizzly':[m
[32m+[m[32m            obstacle.width = 70;[m
[32m+[m[32m            obstacle.height = 70;[m
[32m+[m[32m            break;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Only add obstacle if there's enough space[m
[32m+[m[32m    const lastObstacle = obstacles[obstacles.length - 1];[m
[32m+[m[32m    if (!lastObstacle || lastObstacle.x < CANVAS_WIDTH - 300) {[m
[32m+[m[32m        obstacles.push(obstacle);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isColliding(a, b) {[m
[32m+[m[32m    // Add a small buffer for more forgiving collisions[m
[32m+[m[32m    const buffer = 10;  // Increased from 5 to 10 for even more forgiving collisions[m
[32m+[m[32m    return (a.x + buffer) < (b.x + b.width) &&[m
[32m+[m[32m           (a.x + a.width - buffer) > b.x &&[m
[32m+[m[32m           (a.y + buffer) < (b.y + b.height) &&[m
[32m+[m[32m           (a.y + a.height - buffer) > b.y;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction gameOver() {[m
[32m+[m[32m    gameState.running = false;[m
[32m+[m[41m    [m
[32m+[m[32m    // Create explosion of particles for game over effect[m
[32m+[m[32m    createParticleBurst(daisy.x + daisy.width/2, daisy.y + daisy.height/2, 50, '#FF0000');[m
[32m+[m[41m    [m
[32m+[m[32m    if (gameState.score > gameState.highScore) {[m
[32m+[m[32m        gameState.highScore = gameState.score;[m
[32m+[m[32m        document.getElementById('highscore').textContent = Math.floor(gameState.highScore);[m
[32m+[m[32m        showMessage("NEW 320 RECORD! PRESS ANY KEY", 0);[m
[32m+[m[32m    } else {[m
[32m+[m[32m        // Use 320-themed running terminology for game over[m
[32m+[m[32m        const gameOverMessages = [[m
[32m+[m[32m            "TRACK CLOSED! PRESS ANY KEY",[m
[32m+[m[32m            "320 METERS TOO FAR! PRESS ANY KEY",[m
[32m+[m[32m            "WINGATE WIPEOUT! PRESS ANY KEY",[m
[32m+[m[32m            "TRACK TUMBLE! PRESS ANY KEY"[m
[32m+[m[32m        ];[m
[32m+[m[32m        showMessage(gameOverMessages[Math.floor(Math.random() * gameOverMessages.length)], 0);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction showMessage(text, duration = 0) {[m
[32m+[m[32m    const messageEl = document.getElementById('game-message');[m
[32m+[m[32m    messageEl.textContent = text;[m
[32m+[m[32m    messageEl.style.display = 'block';[m
[32m+[m[41m    [m
[32m+[m[32m    // Add animation effect[m
[32m+[m[32m    messageEl.style.animation = 'none';[m
[32m+[m[32m    setTimeout(() => {[m
[32m+[m[32m        messageEl.style.animation = 'pulse 2s infinite';[m
[32m+[m[32m    }, 10);[m
[32m+[m[41m    [m
[32m+[m[32m    if (duration > 0) {[m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            messageEl.style.display = 'none';[m
[32m+[m[32m        }, duration);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction hideMessage() {[m
[32m+[m[32m    document.getElementById('game-message').style.display = 'none';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Add key listener for using gels with 320 theme[m
[32m+[m[32mdocument.addEventListener('keydown', (event) => {[m
[32m+[m[32m    if (gameState.paused) {[m
[32m+[m[32m        if (event.code === 'KeyR') {[m
[32m+[m[32m            togglePause();[m
[32m+[m[32m        }[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    if (event.code === 'KeyJ' && daisy.gels > 0 && !daisy.isInvincible) {[m
[32m+[m[32m        activateInvincibility();[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Add animation keyframes[m
[32m+[m[32mdocument.head.insertAdjacentHTML('beforeend', `[m
[32m+[m[32m<style>[m
[32m+[m[32m@keyframes pulse {[m
[32m+[m[32m    0% { transform: translate(-50%, -50%) scale(1); }[m
[32m+[m[32m    50% { transform: translate(-50%, -50%) scale(1.05); }[m
[32m+[m[32m    100% { transform: translate(-50%, -50%) scale(1); }[m
[32m+[m[32m}[m
[32m+[m[32m</style>[m
[32m+[m[32m`);[m
[32m+[m
[32m+[m[32m// Add touch controls for mobile[m
[32m+[m[32mfunction handleTouchStart(e) {[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m[41m    [m
[32m+[m[32m    if (!gameState.running) {[m
[32m+[m[32m        startGame();[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    if (gameState.paused) return;[m
[32m+[m[41m    [m
[32m+[m[32m    // Simple tap to jump[m
[32m+[m[32m    if (!daisy.isJumping) {[m
[32m+[m[32m        jump();[m
[32m+[m[32m        daisy.canDoubleJump = true;[m
[32m+[m[32m    } else if (daisy.canDoubleJump) {[m
[32m+[m[32m        // Double jump with reduced force[m
[32m+[m[32m        daisy.velocityY = JUMP_FORCE * 0.7;[m
[32m+[m[32m        daisy.canDoubleJump = false;[m
[32m+[m[41m        [m
[32m+[m[32m        // Double jump particles (yellow)[m
[32m+[m[32m        createParticleBurst(daisy.x + daisy.width/2, daisy.y + daisy.height/2, 12, '#FFD700');[m
[32m+[m[41m        [m
[32m+[m[32m        // Show double jump message with 320-themed running terminology[m
[32m+[m[32m        showMessage("SECOND WIND! 320 POWER!", 500);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction handleTouchEnd(e) {[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Add mobile control buttons[m
[32m+[m[32mfunction addMobileControls() {[m
[32m+[m[32m    // Create mobile control container[m
[32m+[m[32m    const controlsContainer = document.createElement('div');[m
[32m+[m[32m    controlsContainer.className = 'mobile-controls';[m
[32m+[m[32m    controlsContainer.style.display = 'none'; // Hide by default, show on small screens[m
[32m+[m[41m    [m
[32m+[m[32m    // Jump button[m
[32m+[m[32m    const jumpBtn = document.createElement('button');[m
[32m+[m[32m    jumpBtn.textContent = 'JUMP';[m
[32m+[m[32m    jumpBtn.className = 'mobile-btn jump-btn';[m
[32m+[m[32m    jumpBtn.addEventListener('touchstart', function(e) {[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[32m        if (!gameState.paused && gameState.running) {[m
[32m+[m[32m            if (!daisy.isJumping) {[m
[32m+[m[32m                jump();[m
[32m+[m[32m                daisy.canDoubleJump = true;[m
[32m+[m[32m            } else if (daisy.canDoubleJump) {[m
[32m+[m[32m                daisy.velocityY = JUMP_FORCE * 0.7;[m
[32m+[m[32m                daisy.canDoubleJump = false;[m
[32m+[m[32m                createParticleBurst(daisy.x + daisy.width/2, daisy.y + daisy.height/2, 12, '#FFD700');[m
[32m+[m[32m                showMessage("SECOND WIND! 320 POWER!", 500);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Kick button[m
[32m+[m[32m    const kickBtn = document.createElement('button');[m
[32m+[m[32m    kickBtn.textContent = 'KICK';[m
[32m+[m[32m    kickBtn.className = 'mobile-btn kick-btn';[m
[32m+[m[32m    kickBtn.addEventListener('touchstart', function(e) {[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[32m        if (!gameState.paused && gameState.running) {[m
[32m+[m[32m            kick();[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Gel button[m
[32m+[m[32m    const gelBtn = document.createElement('button');[m
[32m+[m[32m    gelBtn.textContent = 'GEL';[m
[32m+[m[32m    gelBtn.className = 'mobile-btn gel-btn';[m
[32m+[m[32m    gelBtn.addEventListener('touchstart', function(e) {[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[32m        if (!gameState.paused && gameState.running) {[m
[32m+[m[32m            if (daisy.gels > 0 && !daisy.isInvincible) {[m
[32m+[m[32m                activateInvincibility();[m
[32m+[m[32m            } else if (gameState.score >= GEL_COST) {[m
[32m+[m[32m                buyGel();[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Pause/Resume button for mobile[m
[32m+[m[32m    const pauseResumeBtn = document.createElement('button');[m
[32m+[m[32m    pauseResumeBtn.textContent = 'PAUSE';[m
[32m+[m[32m    pauseResumeBtn.className = 'mobile-btn pause-resume-btn';[m
[32m+[m[32m    pauseResumeBtn.addEventListener('touchstart', function(e) {[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[32m        togglePause();[m
[32m+[m[32m        this.textContent = gameState.paused ? "RESUME" : "PAUSE";[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Add buttons to container[m
[32m+[m[32m    controlsContainer.appendChild(jumpBtn);[m
[32m+[m[32m    controlsContainer.appendChild(kickBtn);[m
[32m+[m[32m    controlsContainer.appendChild(gelBtn);[m
[32m+[m[32m    controlsContainer.appendChild(pauseResumeBtn);[m
[32m+[m[41m    [m
[32m+[m[32m    // Add container to game[m
[32m+[m[32m    document.querySelector('.game-container').appendChild(controlsContainer);[m
[32m+[m[41m    [m
