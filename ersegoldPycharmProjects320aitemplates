[32m+[m[32m{% block extra_js %}[m
[32m+[m[32m<script>[m
[32m+[m[32mdocument.addEventListener('DOMContentLoaded', function() {[m
[32m+[m[32m    // Carousel rotation with captions[m
[32m+[m[32m    const carousel = document.querySelector('.featured-carousel');[m
[32m+[m[32m    if (carousel) {[m
[32m+[m[32m        const images = carousel.querySelectorAll('img');[m
[32m+[m[32m        const captions = carousel.querySelectorAll('.image-caption');[m
[32m+[m[32m        let currentIndex = 0;[m
[32m+[m
[32m+[m[32m        setInterval(() => {[m
[32m+[m[32m            images[currentIndex].classList.remove('active');[m
[32m+[m[32m            captions[currentIndex].style.opacity = '0';[m
[32m+[m[32m            currentIndex = (currentIndex + 1) % images.length;[m
[32m+[m[32m            images[currentIndex].classList.add('active');[m
[32m+[m[32m            captions[currentIndex].style.opacity = '1';[m
[32m+[m[32m        }, 4000);[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m[32m</script>[m
[32m+[m[32m{% endblock %}[m[41m [m
