[32m+[m[32m{% block extra_js %}[m
[32m+[m[32m<script>[m
[32m+[m[32mfunction validateInput(questionId) {[m
[32m+[m[32m    // Only validate question 1 (name is required)[m
[32m+[m[32m    if (questionId === 'q1') {[m
[32m+[m[32m        const input = document.getElementById('userName');[m
[32m+[m[32m        return input.value.trim() !== '';[m
[32m+[m[32m    }[m
[32m+[m[32m    return true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction nextQuestion(currentQuestionNum) {[m
[32m+[m[32m    const currentQuestion = document.getElementById(`q${currentQuestionNum}`);[m
[32m+[m[41m    [m
[32m+[m[32m    // Only validate name field (q1)[m
[32m+[m[32m    if (currentQuestionNum === 1 && !validateInput('q1')) {[m
[32m+[m[32m        alert('Please enter your name to continue.');[m
[32m+[m[32m        return;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    const nextQuestionNum = currentQuestionNum + 1;[m
[32m+[m[32m    const nextQuestion = document.getElementById(`q${nextQuestionNum}`);[m
[32m+[m[41m    [m
[32m+[m[32m    if (nextQuestion) {[m
[32m+[m[32m        currentQuestion.classList.remove('active');[m
[32m+[m[32m        nextQuestion.classList.add('active');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction prevQuestion(currentQuestionNum) {[m
[32m+[m[32m    const currentQuestion = document.getElementById(`q${currentQuestionNum}`);[m
[32m+[m[32m    const prevQuestionNum = currentQuestionNum - 1;[m
[32m+[m[32m    const prevQuestion = document.getElementById(`q${prevQuestionNum}`);[m
[32m+[m[41m    [m
[32m+[m[32m    if (prevQuestion) {[m
[32m+[m[32m        currentQuestion.classList.remove('active');[m
[32m+[m[32m        prevQuestion.classList.add('active');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32masync function submitForm() {[m
[32m+[m[32m    const formData = {[m
[32m+[m[32m        name: document.getElementById('userName').value,[m
[32m+[m[32m        miles_past_320_days: document.getElementById('miles_past_320_days').value,[m
[32m+[m[32m        kilometers_2025: document.getElementById('kilometers_2025').value,[m
[32m+[m[32m        avg_hr_walking: document.getElementById('avg_hr_walking').value,[m
[32m+[m[32m        recent_haircut: document.getElementById('recent_haircut').value,[m
[32m+[m[32m        iron_levels: document.getElementById('iron_levels').value,[m
[32m+[m[32m        last_race_date: document.getElementById('last_race_date').value,[m
[32m+[m[32m        last_date_race: document.getElementById('last_date_race').value,[m
[32m+[m[32m        ssn_digit_sum: document.getElementById('ssn_digit_sum').value[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Hide form, show loading[m
[32m+[m[32m    document.getElementById('questionnaireForm').style.display = 'none';[m
[32m+[m[32m    const loadingSection = document.getElementById('loadingSection');[m
[32m+[m[32m    loadingSection.style.display = 'block';[m
[32m+[m
[32m+[m[32m    // Fake loading sequence[m
[32m+[m[32m    const progressBar = document.querySelector('.progress-bar');[m
[32m+[m[32m    const statusDiv = document.getElementById('calculationStatus');[m
[32m+[m[32m    const statuses = [[m
[32m+[m[32m        "Analyzing track conditions...",[m
[32m+[m[32m        "Processing community insights...",[m
[32m+[m[32m        "Calibrating workout parameters...",[m
[32m+[m[32m        "Validating with track veterans...",[m
[32m+[m[32m        "Optimizing for 320m segments...",[m
[32m+[m[32m        "Fine-tuning intensity levels...",[m
[32m+[m[32m        "Preparing your custom session..."[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    for (let i = 0; i < statuses.length; i++) {[m
[32m+[m[32m        statusDiv.textContent = statuses[i];[m
[32m+[m[32m        progressBar.style.width = `${(i + 1) * (100 / statuses.length)}%`;[m
[32m+[m[32m        await new Promise(r => setTimeout(r, 800));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m        const response = await fetch('/generate_workout', {[m
[32m+[m[32m            method: 'POST',[m
[32m+[m[32m            headers: {[m
[32m+[m[32m                'Content-Type': 'application/json'[m
[32m+[m[32m            },[m
[32m+[m[32m            body: JSON.stringify(formData)[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        const data = await response.json();[m
[32m+[m
[32m+[m[32m        // Hide loading, show results[m
[32m+[m[32m        loadingSection.style.display = 'none';[m
[32m+[m[32m        const resultDiv = document.getElementById('result');[m
[32m+[m[32m        resultDiv.style.display = 'block';[m
[32m+[m
[32m+[m[32m        // Update result template[m
[32m+[m[32m        resultDiv.innerHTML = `[m
[32m+[m[32m            <div class="alert" style="background: rgba(0, 0, 0, 0.7); border: 1px solid rgba(52, 152, 219, 0.3); border-radius: 12px; color: white; padding: 2rem;">[m
[32m+[m[32m                <div class="track-badge mb-3">SESSION ${Math.floor(Math.random() * 900000) + 100000}</div>[m
[32m+[m[32m                <h4 class="alert-heading mb-4">${data.workout_intro}</h4>[m
[32m+[m[32m                <div class="workout-text">[m
[32m+[m[32m                    <h3 class="text-gradient mb-3">${data.workout_details.intensity}</h3>[m
[32m+[m[32m                    ${data.workout_details.workout.replace(/\n/g, '<br>')}[m
[32m+[m[32m                </div>[m
[32m+[m[32m                <hr style="border-color: rgba(52, 152, 219, 0.3);">[m
[32m+[m[32m                <p><em>${data.workout_details.science}</em></p>[m
[32m+[m[32m                <p class="mb-0"><small class="text-uppercase" style="letter-spacing: 1px;">${data.ai_insight}</small></p>[m
[32m+[m[32m                <div class="mt-3" style="color: rgba(255, 255, 255, 0.6); font-size: 0.8rem;">[m
[32m+[m[32m                    <span class="text-uppercase">Custom Workout</span> •[m[41m [m
[32m+[m[32m                    Validated by ${data.meta.models_consulted} track club members •[m[41m [m
[32m+[m[32m                    Analysis time: ${data.meta.processing_time_ms}ms[m
[32m+[m[32m                </div>[m
[32m+[m[32m            </div>[m
[32m+[m[32m        `;[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m        console.error('Error:', error);[m
[32m+[m[32m        alert('Error generating workout. Please try again.');[m
[32m+[m[32m        loadingSection.style.display = 'none';[m
[32m+[m[32m        document.getElementById('questionnaireForm').style.display = 'block';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    document.getElementById('homeButton').style.display = 'block';[m
[32m+[m[32m}[m
[32m+[m[32m</script>[m
[32m+[m[32m{% endblock %}[m
[32m+[m
[32m+[m[32m{% endblock %}[m
