[32m+[m[32m{% block extra_js %}[m
[32m+[m[32m<script>[m
[32m+[m[32m// Single initialization point[m
[32m+[m[32mdocument.addEventListener('DOMContentLoaded', function() {[m
[32m+[m[32m    console.log('DOM Content Loaded');[m
[32m+[m[41m    [m
[32m+[m[32m    // Show popup immediately after page load[m
[32m+[m[32m    setTimeout(() => {[m
[32m+[m[32m        const popup = document.getElementById('signupPopup');[m
[32m+[m[32m        if (popup) {[m
[32m+[m[32m            popup.style.display = 'flex';[m
[32m+[m[32m            setTimeout(() => {[m
[32m+[m[32m                popup.classList.add('active');[m
[32m+[m[32m                document.body.style.overflow = 'hidden';[m
[32m+[m[32m                console.log('Popup activated');[m
[32m+[m[32m            }, 100);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            console.error('Popup element not found');[m
[32m+[m[32m        }[m
[32m+[m[32m    }, 1500);[m
[32m+[m
[32m+[m[32m    // Event Listeners[m
[32m+[m[32m    const popupForm = document.getElementById('popupSignupForm');[m
[32m+[m[32m    if (popupForm) {[m
[32m+[m[32m        popupForm.addEventListener('submit', handlePopupSubmit);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    const signupPopup = document.getElementById('signupPopup');[m
[32m+[m[32m    if (signupPopup) {[m
[32m+[m[32m        signupPopup.addEventListener('click', function(e) {[m
[32m+[m[32m            if (e.target === this) {[m
[32m+[m[32m                closePopup();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Close button functionality[m
[32m+[m[32m    const closeButton = document.querySelector('.signup-popup-close');[m
[32m+[m[32m    if (closeButton) {[m
[32m+[m[32m        closeButton.addEventListener('click', function(e) {[m
[32m+[m[32m            e.preventDefault();[m
[32m+[m[32m            closePopup();[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mfunction closePopup() {[m
[32m+[m[32m    const popup = document.getElementById('signupPopup');[m
[32m+[m[32m    if (popup) {[m
[32m+[m[32m        popup.classList.remove('active');[m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            popup.style.display = 'none';[m
[32m+[m[32m            document.body.style.overflow = '';[m
[32m+[m[32m        }, 300);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Name transformation animation[m
[32m+[m[32mdocument.querySelector('.transform-trigger').addEventListener('click', function() {[m
[32m+[m[32m    const letters = document.querySelectorAll('.letter');[m
[32m+[m[32m    const originalLetters = Array.from(letters).filter(l => l.dataset.original);[m
[32m+[m[32m    const targetLetters = Array.from(letters).filter(l => l.dataset.target);[m
[32m+[m[41m    [m
[32m+[m[32m    // First phase: flip original letters[m
[32m+[m[32m    originalLetters.forEach((letter, i) => {[m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            letter.classList.add('transforming');[m
[32m+[m[32m        }, i * 200);[m
[32m+[m[32m    });[m
[32m+[m[41m    [m
[32m+[m[32m    // Second phase: reveal target letters[m
[32m+[m[32m    setTimeout(() => {[m
[32m+[m[32m        originalLetters.forEach(letter => letter.style.opacity = '0');[m
[32m+[m[32m        targetLetters.forEach((letter, i) => {[m
[32m+[m[32m            setTimeout(() => {[m
[32m+[m[32m                letter.classList.add('transforming');[m
[32m+[m[32m                letter.style.opacity = '1';[m
[32m+[m[32m            }, i * 200);[m
[32m+[m[32m        });[m
[32m+[m[32m    }, originalLetters.length * 200 + 500);[m
[32m+[m[41m    [m
[32m+[m[32m    // Reset after animation[m
[32m+[m[32m    setTimeout(() => {[m
[32m+[m[32m        letters.forEach(letter => {[m
[32m+[m[32m            letter.classList.remove('transforming');[m
[32m+[m[32m            letter.style.opacity = '1';[m
[32m+[m[32m        });[m
[32m+[m[32m    }, (originalLetters.length + targetLetters.length) * 200 + 1500);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Real-time stats update[m
[32m+[m[32mfunction updateStats() {[m
[32m+[m[32m    const bars = document.querySelectorAll('.stat-bar');[m
[32m+[m[32m    bars.forEach(bar => {[m
[32m+[m[32m        const newValue = Math.floor(Math.random() * 30) + 70; // Random value between 70-100[m
[32m+[m[32m        bar.style.setProperty('--value', newValue);[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Update stats every 3 seconds[m
[32m+[m[32msetInterval(updateStats, 3000);[m
[32m+[m
[32m+[m[32m// Initialize circular progress[m
[32m+[m[32mdocument.addEventListener('DOMContentLoaded', function() {[m
[32m+[m[32m    const progress = document.querySelector('.progress-bar');[m
[32m+[m[32m    if (progress) {[m
[32m+[m[32m        progress.style.strokeDasharray = '100, 100';[m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            progress.style.strokeDasharray = '75, 100';[m
[32m+[m[32m        }, 100);[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Email signup form handler[m
[32m+[m[32mdocument.getElementById('emailSignupForm').addEventListener('submit', function(e) {[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m[32m    const emailInput = document.getElementById('signupEmail');[m
[32m+[m[32m    const submitButton = this.querySelector('button[type="submit"]');[m
[32m+[m[32m    const messageDiv = document.getElementById('signupMessage');[m
[32m+[m[32m    const email = emailInput.value;[m
[32m+[m
[32m+[m[32m    // Disable form while submitting[m
[32m+[m[32m    emailInput.disabled = true;[m
[32m+[m[32m    submitButton.disabled = true;[m
[32m+[m[32m    submitButton.innerHTML = 'Processing...';[m
[32m+[m[32m    messageDiv.style.display = 'none';[m
[32m+[m
[32m+[m[32m    // Send to backend[m
[32m+[m[32m    fetch('/subscribe', {[m
[32m+[m[32m        method: 'POST',[m
[32m+[m[32m        headers: {[m
[32m+[m[32m            'Content-Type': 'application/json',[m
[32m+[m[32m        },[m
[32m+[m[32m        body: JSON.stringify({ email: email })[m
[32m+[m[32m    })[m
[32m+[m[32m    .then(response => response.json())[m
[32m+[m[32m    .then(data => {[m
[32m+[m[32m        messageDiv.style.display = 'block';[m
[32m+[m[32m        if (data.success) {[m
[32m+[m[32m            messageDiv.className = 'alert alert-success mt-3';[m
[32m+[m[32m            messageDiv.innerHTML = 'You have been added to the waiting list! We will notify you when a spot becomes available.';[m
[32m+[m[32m            this.reset();[m
[32m+[m[32m        } else {[m
[32m+[m[32m            if (data.message.includes('already subscribed')) {[m
[32m+[m[32m                messageDiv.className = 'alert alert-info mt-3';[m
[32m+[m[32m                messageDiv.innerHTML = 'You are already on the waiting list! We will notify you when a spot becomes available.';[m
[32m+[m[32m            } else {[m
[32m+[m[32m                messageDiv.className = 'alert alert-danger mt-3';[m
[32m+[m[32m                messageDiv.innerHTML = data.message || 'Something went wrong. Please try again.';[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    })[m
[32m+[m[32m    .catch(error => {[m
[32m+[m[32m        messageDiv.style.display = 'block';[m
[32m+[m[32m        messageDiv.className = 'alert alert-danger mt-3';[m
[32m+[m[32m        messageDiv.innerHTML = 'Error subscribing. Please try again.';[m
[32m+[m[32m        console.error('Error:', error);[m
[32m+[m[32m    })[m
[32m+[m[32m    .finally(() => {[m
[32m+[m[32m        // Re-enable form[m
[32m+[m[32m        emailInput.disabled = false;[m
[32m+[m[32m        submitButton.disabled = false;[m
[32m+[m[32m        submitButton.innerHTML = 'Subscribe';[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Updated Modal Functions[m
[32m+[m[32mdocument.addEventListener('DOMContentLoaded', function() {[m
[32m+[m[32m    // Show modal after 2 seconds[m
[32m+[m[32m    setTimeout(openSignupModal, 2000);[m
[32m+[m
[32m+[m[32m    // Make sure all "Subscribe" and "Join" buttons open the modal[m
[32m+[m[32m    document.querySelectorAll('.btn-primary').forEach(button => {[m
[32m+[m[32m        if (button.textContent.toLowerCase().includes('subscribe') ||[m[41m [m
[32m+[m[32m            button.textContent.toLowerCase().includes('join')) {[m
[32m+[m[32m            button.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                openSignupModal();[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Handle modal signup form[m
[32m+[m[32m    document.getElementById('modalSignupForm').addEventListener('submit', handleModalSubmit);[m
[32m+[m
[32m+[m[32m    // Close modal when clicking outside[m
[32m+[m[32m    document.getElementById('signupModal').addEventListener('click', function(e) {[m
[32m+[m[32m        if (e.target === this) {[m
[32m+[m[32m            closeSignupModal();[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Update existing email signup to use modal[m
[32m+[m[32m    document.getElementById('emailSignupForm').addEventListener('submit', function(e) {[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[32m        openSignupModal();[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mfunction openSignupModal() {[m
[32m+[m[32m    const modal = document.getElementById('signupModal');[m
[32m+[m[32m    if (modal) {[m
[32m+[m[32m        modal.classList.add('active');[m
[32m+[m[32m        document.body.style.overflow = 'hidden';[m
[32m+[m[32m        // Focus on email input[m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            const emailInput = document.getElementById('modalSignupEmail');[m
[32m+[m[32m            if (emailInput) emailInput.focus();[m
[32m+[m[32m        }, 300);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction closeSignupModal() {[m
[32m+[m[32m    const modal = document.getElementById('signupModal');[m
[32m+[m[32m    if (modal) {[m
[32m+[m[32m        modal.classList.remove('active');[m
[32m+[m[32m        document.body.style.overflow = '';[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction handleModalSubmit(e) {[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m[32m    const emailInput = document.getElementById('modalSignupEmail');[m
[32m+[m[32m    const submitButton = this.querySelector('button[type="submit"]');[m
[32m+[m[32m    const messageDiv = document.getElementById('modalSignupMessage');[m
[32m+[m[32m    const email = emailInput.value;[m
[32m+[m
[32m+[m[32m    // Disable form while submitting[m
[32m+[m[32m    emailInput.disabled = true;[m
[32m+[m[32m    submitButton.disabled = true;[m
[32m+[m[32m    submitButton.innerHTML = 'Processing...';[m
[32m+[m[32m    messageDiv.style.display = 'none';[m
[32m+[m
[32m+[m[32m    // Send to backend[m
[32m+[m[32m    fetch('/subscribe', {[m
[32m+[m[32m        method: 'POST',[m
[32m+[m[32m        headers: {[m
[32m+[m[32m            'Content-Type': 'application/json',[m
[32m+[m[32m        },[m
[32m+[m[32m        body: JSON.stringify({ email: email })[m
[32m+[m[32m    })[m
[32m+[m[32m    .then(response => response.json())[m
[32m+[m[32m    .then(data => {[m
[32m+[m[32m        messageDiv.style.display = 'block';[m
[32m+[m[32m        if (data.success) {[m
[32m+[m[32m            messageDiv.className = 'alert alert-success mt-3';[m
[32m+[m[32m            messageDiv.innerHTML = 'Welcome to the revolution! We\'ll be in touch soon.';[m
[32m+[m[32m            this.reset();[m
[32m+[m[32m            setTimeout(closeSignupModal, 2000);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            messageDiv.className = 'alert alert-info mt-3';[m
[32m+[m[32m            messageDiv.innerHTML = data.message || 'Something went wrong. Please try again.';[m
[32m+[m[32m        }[m
[32m+[m[32m    })[m
[32m+[m[32m    .catch(error => {[m
[32m+[m[32m        messageDiv.style.display = 'block';[m
[32m+[m[32m        messageDiv.className = 'alert alert-danger mt-3';[m
[32m+[m[32m        messageDiv.innerHTML = 'Error subscribing. Please try again.';[m
[32m+[m[32m        console.error('Error:', error);[m
[32m+[m[32m    })[m
[32m+[m[32m    .finally(() => {[m
[32m+[m[32m        // Re-enable form[m
[32m+[m[32m        emailInput.disabled = false;[m
[32m+[m[32m        submitButton.disabled = false;[m
[32m+[m[32m        submitButton.innerHTML = 'Kick the Fourth Leg';[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction handlePopupSubmit(e) {[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m[32m    const form = e.target;[m
[32m+[m[32m    const email = form.email.value;[m
[32m+[m[32m    const submitButton = form.querySelector('button[type="submit"]');[m
[32m+[m[41m    [m
[32m+[m[32m    // Disable form while submitting[m
[32m+[m[32m    form.email.disabled = true;[m
[32m+[m[32m    submitButton.disabled = true;[m
[32m+[m[32m    submitButton.innerHTML = 'Processing...';[m
[32m+[m
[32m+[m[32m    // Send to backend[m
[32m+[m[32m    fetch('/subscribe', {[m
[32m+[m[32m        method: 'POST',[m
[32m+[m[32m        headers: {[m
[32m+[m[32m            'Content-Type': 'application/json',[m
[32m+[m[32m        },[m
[32m+[m[32m        body: JSON.stringify({ email: email })[m
[32m+[m[32m    })[m
[32m+[m[32m    .then(response => response.json())[m
[32m+[m[32m    .then(data => {[m
[32m+[m[32m        if (data.success) {[m
[32m+[m[32m            form.innerHTML = '<div class="alert alert-success">Welcome to the revolution! Check your email for next steps.</div>';[m
[32m+[m[32m            setTimeout(closePopup, 2000);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            const message = data.message || 'Something went wrong. Please try again.';[m
[32m+[m[32m            form.innerHTML = `<div class="alert alert-danger">${message}</div>`;[m
[32m+[m[32m        }[m
[32m+[m[32m    })[m
[32m+[m[32m    .catch(error => {[m
[32m+[m[32m        console.error('Error:', error);[m
[32m+[m[32m        form.innerHTML = '<div class="alert alert-danger">Error subscribing. Please try again.</div>';[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[32m+[m[32m</script>[m
[32m+[m[32m{% endblock %}[m[41m [m
