[32m+[m[32mfrom PIL import Image, ImageDraw[m
[32m+[m[32mimport os[m
[32m+[m
[32m+[m[32mdef create_dog():[m
[32m+[m[32m    img = Image.new('RGBA', (64, 64), (0, 0, 0, 0))[m
[32m+[m[32m    draw = ImageDraw.Draw(img)[m
[32m+[m[41m    [m
[32m+[m[32m    # Body[m
[32m+[m[32m    draw.rectangle([20, 30, 44, 45], fill=(101, 67, 33))  # Brown body[m
[32m+[m[32m    # Head[m
[32m+[m[32m    draw.ellipse([10, 25, 25, 40], fill=(101, 67, 33))  # Brown head[m
[32m+[m[32m    # Tail[m
[32m+[m[32m    draw.polygon([(44, 35), (55, 30), (50, 40)], fill=(101, 67, 33))[m
[32m+[m[32m    # Legs[m
[32m+[m[32m    draw.rectangle([25, 45, 30, 55], fill=(101, 67, 33))  # Front leg[m
[32m+[m[32m    draw.rectangle([35, 45, 40, 55], fill=(101, 67, 33))  # Back leg[m
[32m+[m[32m    # Eye[m
[32m+[m[32m    draw.ellipse([13, 29, 16, 32], fill=(0, 0, 0))[m
[32m+[m[41m    [m
[32m+[m[32m    return img[m
[32m+[m
[32m+[m[32mdef create_bike():[m
[32m+[m[32m    img = Image.new('RGBA', (64, 64), (0, 0, 0, 0))[m
[32m+[m[32m    draw = ImageDraw.Draw(img)[m
[32m+[m[41m    [m
[32m+[m[32m    # Wheels[m
[32m+[m[32m    draw.ellipse([10, 35, 25, 50], outline=(50, 50, 50), width=2)  # Front wheel[m
[32m+[m[32m    draw.ellipse([40, 35, 55, 50], outline=(50, 50, 50), width=2)  # Back wheel[m
[32m+[m[32m    # Frame[m
[32m+[m[32m    draw.line([17, 42, 47, 42], fill=(30, 144, 255), width=3)  # Main frame[m
[32m+[m[32m    draw.line([17, 42, 32, 25], fill=(30, 144, 255), width=3)  # Front frame[m
[32m+[m[32m    draw.line([32, 25, 47, 42], fill=(30, 144, 255), width=3)  # Back frame[m
[32m+[m[32m    # Handlebars[m
[32m+[m[32m    draw.line([32, 25, 32, 20], fill=(30, 144, 255), width=2)[m
[32m+[m[32m    draw.line([27, 20, 37, 20], fill=(30, 144, 255), width=2)[m
[32m+[m[41m    [m
[32m+[m[32m    return img[m
[32m+[m
[32m+[m[32mdef create_child():[m
[32m+[m[32m    img = Image.new('RGBA', (64, 64), (0, 0, 0, 0))[m
[32m+[m[32m    draw = ImageDraw.Draw(img)[m
[32m+[m[41m    [m
[32m+[m[32m    # Head[m
[32m+[m[32m    draw.ellipse([25, 10, 40, 25], fill=(255, 218, 185))[m
[32m+[m[32m    # Body[m
[32m+[m[32m    draw.rectangle([30, 25, 35, 45], fill=(255, 0, 0))  # Red shirt[m
[32m+[m[32m    # Legs[m
[32m+[m[32m    draw.rectangle([30, 45, 33, 55], fill=(0, 0, 255))  # Left leg[m
[32m+[m[32m    draw.rectangle([34, 45, 37, 55], fill=(0, 0, 255))  # Right leg[m
[32m+[m[32m    # Arms[m
[32m+[m[32m    draw.line([25, 30, 30, 35], fill=(255, 218, 185), width=3)  # Left arm[m
[32m+[m[32m    draw.line([35, 35, 40, 30], fill=(255, 218, 185), width=3)  # Right arm[m
[32m+[m[41m    [m
[32m+[m[32m    return img[m
[32m+[m
[32m+[m[32mdef save_obstacles():[m
[32m+[m[32m    # Create directory if it doesn't exist[m
[32m+[m[32m    os.makedirs('static/images/game', exist_ok=True)[m
[32m+[m[41m    [m
[32m+[m[32m    # Create and save obstacles[m
[32m+[m[32m    obstacles = {[m
[32m+[m[32m        'dog': create_dog(),[m
[32m+[m[32m        'bike': create_bike(),[m
[32m+[m[32m        'child': create_child()[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    for name, img in obstacles.items():[m
[32m+[m[32m        img.save(f'static/images/game/{name}.png')[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    save_obstacles()[m[41m [m
