[32m+[m[32m{% block extra_js %}[m
[32m+[m[32m<script>[m
[32m+[m[32m// Add price animation[m
[32m+[m[32msetInterval(() => {[m
[32m+[m[32m    const priceElement = document.querySelector('.price-value');[m
[32m+[m[32m    const currentPrice = parseFloat(priceElement.textContent.substring(1));[m
[32m+[m[32m    const newPrice = currentPrice + (Math.random() - 0.3) * 0.01;[m
[32m+[m[32m    priceElement.textContent = '$' + newPrice.toFixed(8);[m
[32m+[m[32m}, 3200);[m
[32m+[m
[32m+[m[32mfunction initiateSwap() {[m
[32m+[m[32m    const loadingMessages = [[m
[32m+[m[32m        "Initializing quantum tunneling...",[m
[32m+[m[32m        "Calculating stride vectors...",[m
[32m+[m[32m        "Validating with track oracle...",[m
[32m+[m[32m        "Optimizing gas fees using AI...",[m
[32m+[m[32m        "Consulting ancient track gods..."[m
[32m+[m[32m    ];[m
[32m+[m[41m    [m
[32m+[m[32m    let i = 0;[m
[32m+[m[32m    const btn = document.querySelector('.swap-interface .quantum-button');[m
[32m+[m[32m    const originalText = btn.textContent;[m
[32m+[m[41m    [m
[32m+[m[32m    const interval = setInterval(() => {[m
[32m+[m[32m        btn.textContent = loadingMessages[i];[m
[32m+[m[32m        i = (i + 1) % loadingMessages.length;[m
[32m+[m[32m    }, 800);[m
[32m+[m[41m    [m
[32m+[m[32m    setTimeout(() => {[m
[32m+[m[32m        clearInterval(interval);[m
[32m+[m[32m        btn.textContent = "INSUFFICIENT QUANTUM ENERGY";[m
[32m+[m[32m        setTimeout(() => {[m
[32m+[m[32m            btn.textContent = originalText;[m
[32m+[m[32m        }, 2000);[m
[32m+[m[32m    }, 4000);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Matrix background effect[m
[32m+[m[32mconst canvas = document.createElement('canvas');[m
[32m+[m[32mcanvas.style.position = 'fixed';[m
[32m+[m[32mcanvas.style.top = '0';[m
[32m+[m[32mcanvas.style.left = '0';[m
[32m+[m[32mcanvas.style.width = '100%';[m
[32m+[m[32mcanvas.style.height = '100%';[m
[32m+[m[32mcanvas.style.zIndex = '-1';[m
[32m+[m[32mcanvas.style.opacity = '0.05';[m
[32m+[m[32mdocument.getElementById('matrix-bg').appendChild(canvas);[m
[32m+[m[32mconst ctx = canvas.getContext('2d');[m
[32m+[m
[32m+[m[32mfunction resizeCanvas() {[m
[32m+[m[32m    canvas.width = window.innerWidth;[m
[32m+[m[32m    canvas.height = window.innerHeight;[m
[32m+[m[32m}[m
[32m+[m[32mresizeCanvas();[m
[32m+[m[32mwindow.addEventListener('resize', resizeCanvas);[m
[32m+[m
[32m+[m[32mconst chars = '320';[m
[32m+[m[32mconst fontSize = 10;[m
[32m+[m[32mconst columns = canvas.width/fontSize;[m
[32m+[m[32mconst drops = Array(Math.floor(columns)).fill(1);[m
[32m+[m
[32m+[m[32mfunction draw() {[m
[32m+[m[32m    ctx.fillStyle = 'rgba(0, 0, 0, 0.05)';[m
[32m+[m[32m    ctx.fillRect(0, 0, canvas.width, canvas.height);[m
[32m+[m[32m    ctx.fillStyle = '#0F0';[m
[32m+[m[32m    ctx.font = fontSize + 'px monospace';[m
[32m+[m[32m    for(let i = 0; i < drops.length; i++) {[m
[32m+[m[32m        const text = chars[Math.floor(Math.random() * chars.length)];[m
[32m+[m[32m        ctx.fillText(text, i * fontSize, drops[i] * fontSize);[m
[32m+[m[32m        if(drops[i] * fontSize > canvas.height && Math.random() > 0.975)[m
[32m+[m[32m            drops[i] = 0;[m
[32m+[m[32m        drops[i]++;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m[32msetInterval(draw, 33);[m
[32m+[m[32m</script>[m
[32m+[m[32m{% endblock %}[m[41m [m
\ No newline at end of file[m
