[32m+[m[32mimport os[m
[32m+[m[32mimport cairosvg[m
[32m+[m
[32m+[m[32mdef convert_svg_to_png(input_path, output_path):[m
[32m+[m[32m    """Convert SVG file to PNG format."""[m
[32m+[m[32m    cairosvg.svg2png(url=input_path, write_to=output_path)[m
[32m+[m
[32m+[m[32mdef main():[m
[32m+[m[32m    # Create game assets directory if it doesn't exist[m
[32m+[m[32m    os.makedirs('static/images/game', exist_ok=True)[m
[32m+[m[41m    [m
[32m+[m[32m    # Convert track background[m
[32m+[m[32m    convert_svg_to_png([m
[32m+[m[32m        'static/images/game/track.svg',[m
[32m+[m[32m        'static/images/game/track.png'[m
[32m+[m[32m    )[m
[32m+[m[41m    [m
[32m+[m[32m    # Convert Daisy sprite sheet[m
[32m+[m[32m    convert_svg_to_png([m
[32m+[m[32m        'static/images/game/daisy.svg',[m
[32m+[m[32m        'static/images/game/daisy.png'[m
[32m+[m[32m    )[m
[32m+[m[41m    [m
[32m+[m[32m    # Convert obstacle sprite[m
[32m+[m[32m    convert_svg_to_png([m
[32m+[m[32m        'static/images/game/obstacle.svg',[m
[32m+[m[32m        'static/images/game/obstacle.png'[m
[32m+[m[32m    )[m
[32m+[m[41m    [m
[32m+[m[32m    print("Asset conversion complete!")[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    main()[m[41m [m
