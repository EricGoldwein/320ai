[32m+[m[32m<!-- Add Chart.js -->[m
[32m+[m[32m<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>[m
[32m+[m
[32m+[m[32m<script>[m
[32m+[m[32mdocument.addEventListener('DOMContentLoaded', function() {[m
[32m+[m[32m    // Update range input displays[m
[32m+[m[32m    document.querySelectorAll('input[type="range"]').forEach(range => {[m
[32m+[m[32m        range.addEventListener('input', function() {[m
[32m+[m[32m            this.parentElement.querySelector('.value-display').textContent = this.value;[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Initialize charts[m
[32m+[m[32m    let radarChart = null;[m
[32m+[m[32m    let barChart = null;[m
[32m+[m
[32m+[m[32m    // DAISY Calculator Form[m
[32m+[m[32m    document.getElementById('daisyCalculator').addEventListener('submit', function(e) {[m
[32m+[m[32m        e.preventDefault();[m
[32m+[m[41m        [m
[32m+[m[32m        // Get form values[m
[32m+[m[32m        const heartRate = parseInt(document.getElementById('heartRate').value);[m
[32m+[m[32m        const shoeSize = parseInt(document.getElementById('shoeSize').value);[m
[32m+[m[32m        const shoeCount = parseInt(document.getElementById('shoeCount').value);[m
[32m+[m[32m        const astroSign = parseInt(document.getElementById('astroSign').value);[m
[32m+[m[32m        const soundtrack = parseInt(document.getElementById('soundtrack').value);[m
[32m+[m[32m        const iceCream = parseInt(document.getElementById('iceCream').value);[m
[32m+[m
[32m+[m[32m        // Calculate DAISY score (nonsensical formula)[m
[32m+[m[32m        const baseScore = ((heartRate * shoeSize) / 100) + (astroSign * 20) + (soundtrack * 30) +[m[41m [m
[32m+[m[32m                         (shoeCount * 15) + (iceCream * 25);[m
[32m+[m[32m        const quantumFactor = Math.sin(Date.now() / 1000) * 50 + 50; // Adds random variation[m
[32m+[m[32m        const finalScore = Math.min(960, Math.max(0, Math.round(baseScore * (quantumFactor / 50))));[m
[32m+[m
[32m+[m[32m        // Display result[m
[32m+[m[32m        document.getElementById('daisyResult').style.display = 'block';[m
[32m+[m[32m        document.getElementById('daisyScoreValue').textContent = finalScore;[m
[32m+[m
[32m+[m[32m        // Generate interpretation[m
[32m+[m[32m        const interpretations = [[m
[32m+[m[32m            "Your chaos quotient is remarkably stable. Daisy would be confused.",[m
[32m+[m[32m            "You're achieving quantum harmony through controlled instability.",[m
[32m+[m[32m            "Your running entropy is approaching optimal disorder.",[m
[32m+[m[32m            "Warning: Your predictability levels are dangerously high.",[m
[32m+[m[32m            "Congratulations! You've achieved perfect imperfection."[m
[32m+[m[32m        ];[m
[32m+[m[32m        document.getElementById('daisyInterpretation').textContent =[m[41m [m
[32m+[m[32m            interpretations[Math.floor(finalScore / 200)];[m
[32m+[m
[32m+[m[32m        // Update radar chart[m
[32m+[m[32m        const radarData = {[m
[32m+[m[32m            labels: ['Chaos Factor', 'Quantum Alignment', 'Stride Entropy', 'Neural Harmony', 'Time Dilation'],[m
[32m+[m[32m            datasets: [{[m
[32m+[m[32m                label: 'Your DAISY™ Profile',[m
[32m+[m[32m                data: [[m
[32m+[m[32m                    heartRate / 2,[m
[32m+[m[32m                    (shoeSize * shoeCount) / 10,[m
[32m+[m[32m                    astroSign * 8,[m
[32m+[m[32m                    soundtrack * 20,[m
[32m+[m[32m                    shoeCount * 15[m
[32m+[m[32m                ],[m
[32m+[m[32m                fill: true,[m
[32m+[m[32m                backgroundColor: 'rgba(255, 99, 132, 0.2)',[m
[32m+[m[32m                borderColor: 'rgb(255, 99, 132)',[m
[32m+[m[32m                pointBackgroundColor: 'rgb(255, 99, 132)',[m
[32m+[m[32m                pointBorderColor: '#fff',[m
[32m+[m[32m                pointHoverBackgroundColor: '#fff',[m
[32m+[m[32m                pointHoverBorderColor: 'rgb(255, 99, 132)'[m
[32m+[m[32m            }][m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        if (radarChart) {[m
[32m+[m[32m            radarChart.destroy();[m
[32m+[m[32m        }[m
[32m+[m[32m        radarChart = new Chart(document.getElementById('daisyRadarChart'), {[m
[32m+[m[32m            type: 'radar',[m
[32m+[m[32m            data: radarData,[m
[32m+[m[32m            options: {[m
[32m+[m[32m                elements: {[m
[32m+[m[32m                    line: {[m
[32m+[m[32m                        borderWidth: 3[m
[32m+[m[32m                    }[m
[32m+[m[32m                },[m
[32m+[m[32m                scales: {[m
[32m+[m[32m                    r: {[m
[32m+[m[32m                        angleLines: {[m
[32m+[m[32m                            color: 'rgba(255, 255, 255, 0.2)'[m
[32m+[m[32m                        },[m
[32m+[m[32m                        grid: {[m
[32m+[m[32m                            color: 'rgba(255, 255, 255, 0.2)'[m
[32m+[m[32m                        },[m
[32m+[m[32m                        ticks: {[m
[32m+[m[32m                            color: 'rgba(255, 255, 255, 0.8)',[m
[32m+[m[32m                            backdropColor: 'transparent'[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        // Update bar chart[m
[32m+[m[32m        const barData = {[m
[32m+[m[32m            labels: ['Heart', 'Skill', 'Power of Will', 'Pleasure', 'Pain'],[m
[32m+[m[32m            datasets: [{[m
[32m+[m[32m                label: 'DAISY™ Components',[m
[32m+[m[32m                data: [[m
[32m+[m[32m                    heartRate / 2,[m
[32m+[m[32m                    (shoeSize * shoeCount) / 10,[m
[32m+[m[32m                    astroSign * 8,[m
[32m+[m[32m                    soundtrack * 20,[m
[32m+[m[32m                    shoeCount * 15[m
[32m+[m[32m                ],[m
[32m+[m[32m                backgroundColor: [[m
[32m+[m[32m                    'rgba(255, 99, 132, 0.5)',[m
[32m+[m[32m                    'rgba(54, 162, 235, 0.5)',[m
[32m+[m[32m                    'rgba(255, 206, 86, 0.5)',[m
[32m+[m[32m                    'rgba(75, 192, 192, 0.5)',[m
[32m+[m[32m                    'rgba(153, 102, 255, 0.5)'[m
[32m+[m[32m                ],[m
[32m+[m[32m                borderColor: [[m
[32m+[m[32m                    'rgba(255, 99, 132, 1)',[m
[32m+[m[32m                    'rgba(54, 162, 235, 1)',[m
[32m+[m[32m                    'rgba(255, 206, 86, 1)',[m
[32m+[m[32m                    'rgba(75, 192, 192, 1)',[m
[32m+[m[32m                    'rgba(153, 102, 255, 1)'[m
[32m+[m[32m                ],[m
[32m+[m[32m                borderWidth: 1[m
[32m+[m[32m            }][m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        if (barChart) {[m
[32m+[m[32m            barChart.destroy();[m
[32m+[m[32m        }[m
[32m+[m[32m        barChart = new Chart(document.getElementById('daisyBarChart'), {[m
[32m+[m[32m            type: 'bar',[m
[32m+[m[32m            data: barData,[m
[32m+[m[32m            options: {[m
[32m+[m[32m                scales: {[m
[32m+[m[32m                    y: {[m
[32m+[m[32m                        beginAtZero: true,[m
[32m+[m[32m                        grid: {[m
[32m+[m[32m                            color: 'rgba(255, 255, 255, 0.1)'[m
[32m+[m[32m                        },[m
[32m+[m[32m                        ticks: {[m
[32m+[m[32m                            color: '#000000',[m
[32m+[m[32m                            font: {[m
[32m+[m[32m                                size: 14[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    },[m
[32m+[m[32m                    x: {[m
[32m+[m[32m                        grid: {[m
[32m+[m[32m                            color: 'rgba(255, 255, 255, 0.1)'[m
[32m+[m[32m                        },[m
[32m+[m[32m                        ticks: {[m
